                      with_LibTIFF_inc=$fallback_path
                      use_LibTIFF=yes
                   else
                      trace 3 Will use default system include path
                      with_LibTIFF_inc=""
                   fi
                fi
             fi # End of: if ( test $with_LibTIFF_pkgconf = no || notset
          else
             trace 4 Library LibTIFF disabled
          fi

          KnownLibs=$KnownLibs" "LibTIFF
          if ( test X$use_LibTIFF = X )
          then
             use_LibTIFF=check
          else
             ConfiguredLibs=$ConfiguredLibs" "LibTIFF
          fi

          with_LibTIFF_fn=TIFFGetVersion
          with_LibTIFF_prq="Zlib"
          with_LibTIFF_hdr=""
          with_LibTIFF_virtual="no"







