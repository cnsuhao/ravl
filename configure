                                          ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                          use_LibTIFF=yes
                                          with_LibTIFF_library=yes
                                          ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                          use_LibTIFF=check
                                          with_LibTIFF_library=check
                                          ;;
                        * )               use_LibTIFF=yes
                                          ;;
                       esac

else
  trace 4 No --with-LibTIFF-library specified
                       with_LibTIFF_library=notset


fi


# Check whether --with-LibTIFF-include was given.
if test "${with_LibTIFF_include+set}" = set; then :
  withval=$with_LibTIFF_include; case $with_LibTIFF-include in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                          use_LibTIFF=no
                                          with_LibTIFF_include=no
                                          ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                          use_LibTIFF=yes
                                          with_LibTIFF_include=yes
                                          ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                          use_LibTIFF=check
                                          with_LibTIFF_include=check
                                          ;;
                        * )               use_LibTIFF=yes
                                          ;;
                       esac

else
  trace 4 No --with-LibTIFF-include specified
                       with_LibTIFF_include=notset


fi

          # Having read the command-line options, check none are mutually
          # exclusive or contradictory
          #
          # First test --with-LibTIFF compatability with the rest
          if ( test $with_LibTIFF = no )
          then
             if ( test $with_LibTIFF_pkgconf != no && test $with_LibTIFF_pkgconf != notset )
             then
                # --with-LibTIFF-pkgconf=path (is neither no or notset and cannot
                # be yes or check as they would have errored above.
                { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "Asking to LibTIFF via pkg-config but --with-LibTIFF disabled it
See \`config.log' for more details." "$LINENO" 5; }
             fi

             if ( test with_LibTIFF_library != no && test $with_LibTIFF_library != notset )
             then
                if ( test with_LibTIFF_library = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF specifically disables library but --with-LibTIFF-library" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF specifically disables library but --with-LibTIFF-library" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-LibTIFF-library ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-LibTIFF-library ignored" >&2;}
                   use_LibTIFF=no
                else
                   # --with-LibTIFF-library is yes or points to the library
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-library asking to use LibTIFF but --with-LibTIFF disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
              fi

             if ( test with_LibTIFF_include != no && test $with_LibTIFF_include != notset )
             then
                if ( test with_LibTIFF_include = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF specifically disables library but --with-LibTIFF-include" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF specifically disables library but --with-LibTIFF-include" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-LibTIFF-include ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-LibTIFF-include ignored" >&2;}
                   use_LibTIFF=no
                else
                   # --with-LibTIFF-include is yes or points to the include
                   # directory
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-include asking to use LibTIFF but --with-LibTIFF disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

          else
             # test --with-LibTIFF != no

             if ( test $with_LibTIFF = check )
             then
                if ( test $with_LibTIFF_pkgconf != no && test $with_LibTIFF_pkgconf != notset )
                then
                   # --with-LibTIFF-pkgconf must point to the metafile
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for LibTIFF via pkg-config but --with-LibTIFF is" >&5
$as_echo "$as_me: WARNING: Explicitly asking for LibTIFF via pkg-config but --with-LibTIFF is" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-LibTIFF ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-LibTIFF ignored." >&2;}
                   use_LibTIFF=yes
                fi

                if ( test with_LibTIFF_library != check && test $with_LibTIFF_library != notset )
                then
                   if ( test with_LibTIFF_library = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF but --with-LibTIFF-library" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF but --with-LibTIFF-library" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-LibTIFF ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-LibTIFF ignored." >&2;}
                      use_LibTIFF=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for LibTIFF via --with-LibTIFF-library but --with-LibTIFF" >&5
$as_echo "$as_me: WARNING: Explicitly asking for LibTIFF via --with-LibTIFF-library but --with-LibTIFF" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-LibTIFF ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-LibTIFF ignored." >&2;}
                      use_LibTIFF=yes
                   fi
                fi

                if ( test with_LibTIFF_include != check && test $with_LibTIFF_include != notset )
                then
                   if ( test with_LibTIFF_include = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF but --with-LibTIFF-include" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF but --with-LibTIFF-include" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-LibTIFF ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-LibTIFF ignored." >&2;}
                      use_LibTIFF=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for LibTIFF via --with-LibTIFF-include but --with-LibTIFF" >&5
$as_echo "$as_me: WARNING: Explicitly asking for LibTIFF via --with-LibTIFF-include but --with-LibTIFF" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-LibTIFF ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-LibTIFF ignored." >&2;}
                      use_LibTIFF=yes
                   fi
                fi

             else
                # --with-LibTIFF != no && != check
                if ( test $with_LibTIFF != notset )
                then
                   # --with-LibTIFF = path or yes

                   # We need not check --with-LibTIFF-pkgconf as there is no
                   # possible conflict (cannot be check and we take no to
                   # be a request to disable pkg-config, rather than the
                   # library as a whole)

                   if ( test $with_LibTIFF_library = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-library disabling LibTIFF but --with-LibTIFF enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test $with_LibTIFF_library = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling LibTIFF via --with-LibTIFF but --with-LibTIFF-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling LibTIFF via --with-LibTIFF but --with-LibTIFF-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-LibTIFF-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-LibTIFF-include ignored." >&2;}
                         use_LibTIFF=no
                      fi
                   fi

                   if ( test $with_LibTIFF_include = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-include disabling LibTIFF but --with-LibTIFF enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test $with_LibTIFF_include = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling LibTIFF via --with-LibTIFF but --with-LibTIFF-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling LibTIFF via --with-LibTIFF but --with-LibTIFF-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-LibTIFF-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-LibTIFF-include ignored." >&2;}
                         use_LibTIFF=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-LibTIFF-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_LibTIFF_pkgconf != notset && test $with_LibTIFF_pkgconf != no )
          then
             # --with-LibTIFF-pkgconf = path (cannot be yes or check)
             if ( test $with_LibTIFF_library = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-library asking to check for LibTIFF but --with-LibTIFF-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-library asking to check for LibTIFF but --with-LibTIFF-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-LibTIFF-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-LibTIFF-library ignored." >&2;}
                use_LibTIFF=yes
             else
                if ( test $with_LibTIFF_library != no && test $with_LibTIFF_library != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-library enabling compile check for LibTIFF but --with-LibTIFF-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test $with_LibTIFF_include = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-include asking to check for LibTIFF but --with-LibTIFF-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-include asking to check for LibTIFF but --with-LibTIFF-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-LibTIFF-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-LibTIFF-include ignored." >&2;}
                use_LibTIFF=yes
             else
                if ( test $with_LibTIFF_include != no && test $with_LibTIFF_include != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-include enabling compile check for LibTIFF but --with-LibTIFF-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test $with_LibTIFF_library = no )
          then
             if ( test $with_LibTIFF_include != no && test $with_LibTIFF_include != notset )
             then
                if ( test $with_LibTIFF_include = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-include asking to check for LibTIFF but --with-LibTIFF-library has" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-include asking to check for LibTIFF but --with-LibTIFF-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-LibTIFF-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-LibTIFF-include ignored." >&2;}
                   use_LibTIFF=no
                else
                   # --with-LibTIFF-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-include enabling LibTIFF but --with-LibTIFF-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-LibTIFF-library != no
             if ( test $with_LibTIFF_library = check )
             then
                if ( test $with_LibTIFF_include = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-library asking to check for LibTIFF but --with-LibTIFF-include has" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-library asking to check for LibTIFF but --with-LibTIFF-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-LibTIFF-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-LibTIFF-library ignored." >&2;}
                   use_LibTIFF=no
                else
                   if ( test $with_LibTIFF_include != check && test $with_LibTIFF_include != notset )
                   then
                      # --with-LibTIFF-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-library asking to check for LibTIFF but --with-LibTIFF-include has" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-library asking to check for LibTIFF but --with-LibTIFF-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-LibTIFF-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-LibTIFF-library ignored." >&2;}
                      use_LibTIFF=yes
                   fi
                fi
             else
                if ( test $with_LibTIFF_library != notset )
                then
                   # --with-LibTIFF-library a path or yes
                   if ( test $with_LibTIFF_include = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-LibTIFF-library enabling LibTIFF but --with-LibTIFF-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test $with_LibTIFF_include = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-include asking to check for LibTIFF but --with-LibTIFF-library has" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-include asking to check for LibTIFF but --with-LibTIFF-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-LibTIFF-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-LibTIFF-include ignored." >&2;}
                         use_LibTIFF=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-LibTIFF to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_LibTIFF != notset && test $with_LibTIFF != no && test $with_LibTIFF != check && test $with_LibTIFF != yes )
          then
             # We have a path to use
             if ( test -d $with_LibTIFF )
             then
                # Parameter is a directory path
                fallback_path=$with_LibTIFF
                trace 3 with-LibTIFF provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_LibTIFF ||
$as_expr X$with_LibTIFF : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_LibTIFF : 'X\(//\)[^/]' \| \
	 X$with_LibTIFF : 'X\(//\)$' \| \
	 X$with_LibTIFF : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_LibTIFF |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_LibTIFF" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_LibTIFF`
                   if ( test X$with_LibTIFF = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-LibTIFF provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-LibTIFF, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-LibTIFF, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-LibTIFF=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-LibTIFF, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-LibTIFF, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_LibTIFF != Xno )
          then
             if ( test $with_LibTIFF_pkgconf = no ||  test $with_LibTIFF_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-LibTIFF-library is a path
                if ( test $with_LibTIFF_library != notset && test $with_LibTIFF_library != no && test $with_LibTIFF_library != check && test $with_LibTIFF_library != yes )
                then
                   # we have a path to use
                   if ( test -d $with_LibTIFF_library )
                   then
                      trace 3 Using --with-LibTIFF-library to set library path for LibTIFF [$with_LibTIFF_library]
                      with_LibTIFF_libpath=$with_LibTIFF_library
                      if ( test X$fallback_path != X && test X$fallback_path != X$with_LibTIFF_libpath )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF-library and --with-LibTIFF specify different paths, --with-LibTIFF ignored" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF-library and --with-LibTIFF specify different paths, --with-LibTIFF ignored" >&2;}
                      fi
                      if ( test X$fallback_file != X )
                      then
                         trace 3 Using file component of --with-LibTIFF to set library name for LibTIFF [$fallback_file]
                         with_LibTIFF_lnklibs=$fallback_file
                      else
                         trace 3 Using default library name for LibTIFF [-ltiff]
                         with_LibTIFF_lnklibs=-ltiff
                      fi
                   else
                      # Path is not to a directory, is it directory/file?
                      path=`$as_dirname -- $with_LibTIFF_library ||
$as_expr X$with_LibTIFF_library : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_LibTIFF_library : 'X\(//\)[^/]' \| \
	 X$with_LibTIFF_library : 'X\(//\)$' \| \
	 X$with_LibTIFF_library : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_LibTIFF_library |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                      if ( test X$path != X && test $path != . )
                      then
                         # --with-LibTIFF-library=/something/file
                         # does the /something point to a directory?
                         if ( test -d $path  )
                         then
                            # We have directory/presumed-library-name
                            # Find candidate library name
                            file=`$EXPR "//$with_LibTIFF_library" : '.*/\([^/]*\)'`
                            if ( test X$with_LibTIFF_library = X$path/$file )
                            then
                               # Parameter is a file path
                               # Does it have a '.a' extension
                               lib=`$EXPR "$file" : '\(.*\)\.a$'`
                               if ( test X$lib != X  )
                               then
                                  lib='-l'`echo $lib | $SED 's/^lib//'`
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-LibTIFF_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-LibTIFF_library pointing to a .a" >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use $lib and" >&5
$as_echo "$as_me: WARNING: Instead we will use $lib and" >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: LDFLAGS=$path" >&5
$as_echo "$as_me: WARNING: LDFLAGS=$path" >&2;}
                               else
                                  # Does it have a '.so' extension
                                  lib=`$EXPR "$file" : '\(.*\)\.so$'`
                                  if ( test X$lib != X )
                                  then
                                     lib='-l'`echo $lib | $SED 's/^lib//'`
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-LibTIFF_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-LibTIFF_library pointing to" >&2;}
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $lib and LDFLAGS=$path" >&5
$as_echo "$as_me: WARNING: $lib and LDFLAGS=$path" >&2;}
                                  else
                                     # Parameter is path and name fragment
                                     lib='-l'`echo $file | $SED 's/^lib//'`
                                     trace 3 $with_LibTIFF_library asks to use $lib and LDFLAGS=$path
                                  fi
                               fi
                               with_LibTIFF_libpath=$path
                               with_LibTIFF_lnklibs=$lib
