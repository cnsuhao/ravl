                      path=`$as_dirname -- $with_posixThreads_library ||
$as_expr X$with_posixThreads_library : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_posixThreads_library : 'X\(//\)[^/]' \| \
	 X$with_posixThreads_library : 'X\(//\)$' \| \
	 X$with_posixThreads_library : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_posixThreads_library |
