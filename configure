             fi
             trace 4 FFMPEGCDEC headers are $CDC_HDR
          fi

          # Now look for the container...
          FMT_HDR=notfound
          if ( test $use_FFMPEGCONT = 'present' )
          then
             trace 5 Processing FFMPEGCONT_CFLAGS [$FFMPEGCONT_CFLAGS] to determine header layout
