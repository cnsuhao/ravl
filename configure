                   use_Zlib=no
                else
                   if ( test $with_Zlib_include != check && test $with_Zlib_include != notset )
                   then
                      # --with-Zlib-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-library asking to check for Zlib but --with-Zlib-include has" >&5
$as_echo "$as_me: WARNING: --with-Zlib-library asking to check for Zlib but --with-Zlib-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-Zlib-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-Zlib-library ignored." >&2;}
                      use_Zlib=yes
                   fi
                fi
             else
                if ( test $with_Zlib_library != notset )
                then
                   # --with-Zlib-library a path or yes
                   if ( test $with_Zlib_include = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Zlib-library enabling Zlib but --with-Zlib-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test $with_Zlib_include = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-include asking to check for Zlib but --with-Zlib-library has" >&5
$as_echo "$as_me: WARNING: --with-Zlib-include asking to check for Zlib but --with-Zlib-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-Zlib-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-Zlib-include ignored." >&2;}
                         use_Zlib=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-Zlib to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_Zlib != notset && test $with_Zlib != no && test $with_Zlib != check && test $with_Zlib != yes )
          then
             # We have a path to use
             if ( test -d $with_Zlib )
             then
                # Parameter is a directory path
                fallback_path=$with_Zlib
                trace 3 with-Zlib provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_Zlib ||
$as_expr X$with_Zlib : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Zlib : 'X\(//\)[^/]' \| \
	 X$with_Zlib : 'X\(//\)$' \| \
	 X$with_Zlib : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Zlib |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_Zlib" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_Zlib`
                   if ( test X$with_Zlib = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-Zlib provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Zlib, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Zlib, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-Zlib=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Zlib, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Zlib, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_Zlib != Xno )
          then
             if ( test $with_Zlib_pkgconf = no ||  test $with_Zlib_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-Zlib-library is a path
                if ( test $with_Zlib_library != notset && test $with_Zlib_library != no && test $with_Zlib_library != check && test $with_Zlib_library != yes )
                then
                   # we have a path to use
                   if ( test -d $with_Zlib_library )
                   then
                      trace 3 Using --with-Zlib-library to set library path for Zlib [$with_Zlib_library]
                      with_Zlib_libpath=$with_Zlib_library
                      if ( test X$fallback_path != X && test X$fallback_path != X$with_Zlib_libpath )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-library and --with-Zlib specify different paths, --with-Zlib ignored" >&5
$as_echo "$as_me: WARNING: --with-Zlib-library and --with-Zlib specify different paths, --with-Zlib ignored" >&2;}
                      fi
                      if ( test X$fallback_file != X )
                      then
                         trace 3 Using file component of --with-Zlib to set library name for Zlib [$fallback_file]
                         with_Zlib_lnklibs=$fallback_file
                      else
                         trace 3 Using default library name for Zlib [-lz]
                         with_Zlib_lnklibs=-lz
                      fi
                   else
                      # Path is not to a directory, is it directory/file?
                      path=`$as_dirname -- $with_Zlib_library ||
$as_expr X$with_Zlib_library : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Zlib_library : 'X\(//\)[^/]' \| \
	 X$with_Zlib_library : 'X\(//\)$' \| \
	 X$with_Zlib_library : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Zlib_library |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                      if ( test X$path != X && test $path != . )
                      then
                         # --with-Zlib-library=/something/file
                         # does the /something point to a directory?
                         if ( test -d $path  )
                         then
                            # We have directory/presumed-library-name
                            # Find candidate library name
                            file=`$EXPR "//$with_Zlib_library" : '.*/\([^/]*\)'`
                            if ( test X$with_Zlib_library = X$path/$file )
                            then
                               # Parameter is a file path
                               # Does it have a '.a' extension
                               lib=`$EXPR "$file" : '\(.*\)\.a$'`
                               if ( test X$lib != X  )
                               then
                                  lib='-l'`echo $lib | $SED 's/^lib//'`
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Zlib_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Zlib_library pointing to a .a" >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use $lib and" >&5
$as_echo "$as_me: WARNING: Instead we will use $lib and" >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: LDFLAGS=$path" >&5
$as_echo "$as_me: WARNING: LDFLAGS=$path" >&2;}
                               else
                                  # Does it have a '.so' extension
                                  lib=`$EXPR "$file" : '\(.*\)\.so$'`
                                  if ( test X$lib != X )
                                  then
                                     lib='-l'`echo $lib | $SED 's/^lib//'`
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Zlib_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-Zlib_library pointing to" >&2;}
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                     { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: $lib and LDFLAGS=$path" >&5
$as_echo "$as_me: WARNING: $lib and LDFLAGS=$path" >&2;}
                                  else
                                     # Parameter is path and name fragment
                                     lib='-l'`echo $file | $SED 's/^lib//'`
                                     trace 3 $with_Zlib_library asks to use $lib and LDFLAGS=$path
                                  fi
                               fi
                               with_Zlib_libpath=$path
                               with_Zlib_lnklibs=$lib
                               use_Zlib=yes
                               if ( test X$fallback_path != X && test X$fallback_path != X$with_Zlib_libpath )
                               then
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-library and --with-Zlib specify different paths, --with-Zlib ignored" >&5
$as_echo "$as_me: WARNING: --with-Zlib-library and --with-Zlib specify different paths, --with-Zlib ignored" >&2;}
                               fi
                               if ( test X$fallback_file != X && test X$fallback_file != X$with_Zlib_lnklibs )
                               then
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-library and --with-Zlib specify different library names, --with-Zlib ignored" >&5
$as_echo "$as_me: WARNING: --with-Zlib-library and --with-Zlib specify different library names, --with-Zlib ignored" >&2;}
                               fi
                            else
                               # Somehow dirname($x)/basename($x) != $x
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Zlib-library, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Zlib-library, parameter ignored" >&2;}
                               if ( test X$fallback_path != X )
                               then
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Zlib value for library path [$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-Zlib value for library path [$fallback_path]" >&2;}
                                  with_Zlib_libpath=$fallback_path
                               else
                                  with_Zlib_libpath=""
                               fi
                               if ( test X$fallback_file != X )
                               then
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Zlib value for library name [$fallback_file]" >&5
$as_echo "$as_me: WARNING: Will use --with-Zlib value for library name [$fallback_file]" >&2;}
                                  with_Zlib_lnklibs=$fallback_file
                               else
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default for library name [-lz]" >&5
$as_echo "$as_me: WARNING: Will use default for library name [-lz]" >&2;}
                                  with_Zlib_lnklibs=-lz
                               fi
                               use_Zlib=yes
                            fi
                         else
                            # --with-Zlib-library=/not-a-dir-path/something
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-Zlib-library, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-Zlib-library, parameter ignored" >&2;}
                            if ( test X$fallback_path != X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Zlib value for library path [$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-Zlib value for library path [$fallback_path]" >&2;}
                               with_Zlib_libpath=$fallback_path
                            else
                               with_Zlib_libpath=""
                            fi
                            if ( test X$fallback_file != X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Zlib value for library name [$fallback_file]" >&5
$as_echo "$as_me: WARNING: Will use --with-Zlib value for library name [$fallback_file]" >&2;}
                               with_Zlib_lnklibs=$fallback_file
                            else
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default for library name [-lz]" >&5
$as_echo "$as_me: WARNING: Will use default for library name [-lz]" >&2;}
                               with_Zlib_lnklibs=-lz
                            fi
                            use_Zlib=yes
                         fi
                      else
                         # --with-Zlib-library=something - assume library name
                         if ( test X$fallback_path != X )
                         then
                            trace 3 No path specified by --with-Zlib-library, using --with-Zlib value [$fallback_path]
                            with_Zlib_libpath=$fallback_path
                         else
                            with_Zlib_libpath=""
                         fi
                         with_Zlib_lnklibs=$with_Zlib_library
                         if ( test X$fallback_file != X && test X$fallback_file != X$with_Zlib_lnklibs )
                         then
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-library and --with-Zlib specify different library names, --with-Zlib ignored" >&5
$as_echo "$as_me: WARNING: --with-Zlib-library and --with-Zlib specify different library names, --with-Zlib ignored" >&2;}
                         fi
                         use_Zlib=yes
                      fi
                   fi
                else
                   # --with-Zlib-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-Zlib-library, using --with-Zlib value [$fallback_path]
                      with_Zlib_libpath=$fallback_path
                      use_Zlib=yes
                   else
                      with_Zlib_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                      trace 3 No library name specified by --with-Zlib-library, using --with-Zlib value [$fallback_file]
                      with_Zlib_lnklibs=$fallback_file
                      use_Zlib=yes
                   else
                      trace 3 No library name specified by --with-Zlib-library, using default [-lz]
                      with_Zlib_lnklibs=-lz
                   fi
                fi

                # Check if --with-Zlib-include is a path
                if ( test $with_Zlib_include != notset && test $with_Zlib_include != no && test $with_Zlib_include != check && test $with_Zlib_include != yes )
                then
                   # we have a path to use
                   if ( test -d $with_Zlib_include )
                   then
                      trace 3 Using --with-Zlib-include to set include path for Zlib [$with_Zlib_include]
                      with_Zlib_inc=$with_Zlib_include
                      if ( test X$fallback_path != X && test X$fallback_path != X$with_Zlib_inc )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib-include and --with-Zlib specify different paths, --with-Zlib ignored" >&5
$as_echo "$as_me: WARNING: --with-Zlib-include and --with-Zlib specify different paths, --with-Zlib ignored" >&2;}
                      fi
                      use_Zlib=yes
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-Zlib-include, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-Zlib-include, parameter ignored" >&2;}
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Zlib value [$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-Zlib value [$fallback_path]" >&2;}
                         with_Zlib_inc=$fallback_path
                         use_Zlib=yes
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_Zlib_inc=""
                      fi
                   fi
                else
                   # No path from --with-Zlib-include; use fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-Zlib-include, using --with-Zlib value [$fallback_path]
                      with_Zlib_inc=$fallback_path
                      use_Zlib=yes
                   else
                      trace 3 Will use default system include path
                      with_Zlib_inc=""
                   fi
                fi
             fi # End of: if ( test $with_Zlib_pkgconf = no || notset
          else
             trace 4 Library Zlib disabled
          fi

          KnownLibs=$KnownLibs" "Zlib
          if ( test X$use_Zlib = X )
          then
             use_Zlib=check
          else
             ConfiguredLibs=$ConfiguredLibs" "Zlib
          fi

          with_Zlib_fn=zlibCompileFlags
          with_Zlib_prq=""
          with_Zlib_hdr=""
          with_Zlib_virtual="no"





