$as_echo "$as_me: WARNING: --with-posixThreads-include and --with-posixThreads specify different paths, --with-posixThreads ignored" >&2;}
                      fi
                      use_posixThreads=yes
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-posixThreads-include, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-posixThreads-include, parameter ignored" >&2;}
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-posixThreads value [$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-posixThreads value [$fallback_path]" >&2;}
                         with_posixThreads_inc=$fallback_path
                         use_posixThreads=yes
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_posixThreads_inc=""
                      fi
                   fi
                else
                   # No path from --with-posixThreads-include; use fallback
                   if ( test X$fallback_path != X )
