                      with_posixThreads_inc=$fallback_path
                      use_posixThreads=yes
                   else
                      trace 3 Will use default system include path
                      with_posixThreads_inc=""
                   fi
                fi
             fi # End of: if ( test $with_posixThreads_pkgconf = no || notset
          else
             trace 4 Library posixThreads disabled
          fi

          KnownLibs=$KnownLibs" "posixThreads
          if ( test X$use_posixThreads = X )
          then
             use_posixThreads=check
          else
             ConfiguredLibs=$ConfiguredLibs" "posixThreads
          fi

          with_posixThreads_fn=pthread_self
          with_posixThreads_prq=""
          with_posixThreads_hdr="pthread.h"
          with_posixThreads_virtual="no"







# Check whether --with-LibTIFF-pkgconf was given.
