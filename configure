                               # Does it have a '.a' extension
                               lib=`$EXPR "$file" : '\(.*\)\.a$'`
                               if ( test X$lib != X  )
                               then
                                  lib='-l'`echo $lib | $SED 's/^lib//'`
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-posixThreads_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-posixThreads_library pointing to a .a" >&2;}
                                  { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
