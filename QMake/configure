                               # Strip any leading -l or lib prefix
                               with_LibJasper_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-LibJasper to set library name for LibJasper [$with_LibJasper_lnklibs]
                   else
                      trace 3 Using default library name for LibJasper ["-ljasper"]
                      with_LibJasper_lnklibs="-ljasper"
                   fi
                fi


                # Check if --with-LibJasper-include is a path parameter
                if ( test "$with_LibJasper_include" != notset && test "$with_LibJasper_include" != no && test "$with_LibJasper_include" != check && test "$with_LibJasper_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_LibJasper_inc=""
                   use_LibJasper=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-LibJasper-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_LibJasper_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for LibJasper
                         with_LibJasper_inc="$with_LibJasper_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-LibJasper-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-LibJasper-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_LibJasper_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-LibJasper value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-LibJasper value [-I$fallback_path]" >&2;}
                         with_LibJasper_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_LibJasper_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibJasper-include and --with-LibJasper specify different paths, --with-LibJasper ignored" >&5
