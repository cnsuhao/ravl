$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Math-library enabling Math but --with-Math-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_Math_include" = check )
