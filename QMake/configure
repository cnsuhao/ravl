$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_tetgen_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-tetgen to set library name for tetgen [$with_tetgen_lnklibs]
                   else
                      trace 3 Using default library name for tetgen ["-ltet"]
                      with_tetgen_lnklibs="-ltet"
                   fi
                fi


                # Check if --with-tetgen-include is a provided
                if ( test "$with_tetgen_include" != notset && test "$with_tetgen_include" != no && test "$with_tetgen_include" != check && test "$with_tetgen_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_tetgen_inc=""
                   use_tetgen=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-tetgen-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_tetgen_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for tetgen
                         with_tetgen_inc="$with_tetgen_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-tetgen-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-tetgen-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_tetgen_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-tetgen value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-tetgen value [-I$fallback_path]" >&2;}
                         with_tetgen_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_tetgen_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-include and --with-tetgen specify different paths, --with-tetgen ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-include and --with-tetgen specify different paths, --with-tetgen ignored" >&2;}
                      fi
                   fi
                else
                   # --with-tetgen-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-tetgen-include, using --with-tetgen value [-I$fallback_path]
                      with_tetgen_inc=-I$fallback_path
                      use_tetgen=yes
                   else
                      trace 3 Will use default system include path
                      with_tetgen_inc=""
                   fi
                fi
             fi # End of: if ( test $with_tetgen_pkgconf = no || notset
          else
             trace 4 Library tetgen disabled
          fi

          KnownLibs=$KnownLibs" "tetgen
          if ( test X$use_tetgen = X )
          then
             use_tetgen=check
             UnconfiguredLibs=$UnconfiguredLibs" "tetgen
          else
             ConfiguredLibs=$ConfiguredLibs" "tetgen
          fi

