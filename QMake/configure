                         # it?
                         if ( test X`$EXPR "X $with_tetgen_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_tetgen_libpath="-L$fallback_path $with_tetgen_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-tetgen-library, using --with-tetgen to set library path for tetgen [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_tetgen_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_tetgen setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_tetgen_libpath $with_tetgen_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library and --with-tetgen specify different libraries, --with-tetgen library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library and --with-tetgen specify different libraries, --with-tetgen library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_tetgen specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_tetgen_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-tetgen-library, using --with-tetgen value [-L$fallback_path]
                            with_tetgen_libpath="-L$fallback_path $with_tetgen_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_tetgen_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-tetgen-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-tetgen to set library path for tetgen [-L$fallback_path]
                      with_tetgen_libpath=-L$fallback_path
                      use_tetgen=yes
                   else
                      with_tetgen_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_tetgen specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_tetgen_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
