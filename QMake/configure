                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Math pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Math pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
