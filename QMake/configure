                                    if ( test X$use_LibJasper = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling LibJasper via --with-LibJasper despite --with-LibJasper-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling LibJasper via --with-LibJasper despite --with-LibJasper-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_LibJasper=yes
                                 fi
                                 with_LibJasper=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-LibJasper checking for LibJasper libraries
                                 if (test X$use_LibJasper = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibJasper asking to check for the libs but --with-LibJasper-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-LibJasper asking to check for the libs but --with-LibJasper-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_LibJasper = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibJasper asking to check for the libs but --with-LibJasper-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-LibJasper asking to check for the libs but --with-LibJasper-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_LibJasper=check
                                    fi
                                 fi
                                 with_LibJasper=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_LibJasper )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_LibJasper
                                    fallback_file=""
                                    trace 3 with-LibJasper provides a fallback path [$fallback_path]
                                    use_LibJasper=yes
                                    with_LibJasper=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_LibJasper ||
$as_expr X$with_LibJasper : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_LibJasper : 'X\(//\)[^/]' \| \
	 X$with_LibJasper : 'X\(//\)$' \| \
	 X$with_LibJasper : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_LibJasper |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_LibJasper" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_LibJasper`
                                       if ( test X$with_LibJasper = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-LibJasper provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_LibJasper=yes
                                          with_LibJasper=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-LibJasper, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-LibJasper, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-LibJasper=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-LibJasper, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-LibJasper, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-LibJasper specified

fi






