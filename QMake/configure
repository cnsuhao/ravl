                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_GLUT_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-GLUT to set library name for GLUT [$with_GLUT_lnklibs]
                   else
                      trace 3 Using default library name for GLUT ["-lglut"]
                      with_GLUT_lnklibs="-lglut"
                   fi
                fi


                # Check if --with-GLUT-include is a provided
                if ( test "$with_GLUT_include" != notset && test "$with_GLUT_include" != no && test "$with_GLUT_include" != check && test "$with_GLUT_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_GLUT_inc=""
                   use_GLUT=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-GLUT-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_GLUT_include " | $SED 's/ -I *//g'`
                   do
