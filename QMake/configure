                               # Strip any leading -l or lib prefix
                               with_vtkGraphics_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-vtkGraphics-library, using --with-vtkGraphics value [$with_vtkGraphics_lnklibs]
                      else
