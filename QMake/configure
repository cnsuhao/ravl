                         trace 3 Using file component of --with-vtkFiltering to set library name for vtkFiltering [$with_vtkFiltering_lnklibs]
                   else
                      trace 3 Using default library name for vtkFiltering ["-lvtkFiltering"]
                      with_vtkFiltering_lnklibs="-lvtkFiltering"
                   fi
                fi


