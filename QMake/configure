as_fn_error "--with-blas-include enabling blas but --with-blas-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-blas-library != no
             if ( test "$with_blas_library" = check )
             then
                if ( test "$with_blas_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-library asking to check for blas but --with-blas-include has" >&5
$as_echo "$as_me: WARNING: --with-blas-library asking to check for blas but --with-blas-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-blas-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-blas-library ignored." >&2;}
                   use_blas=no
                else
                   if ( test "$with_blas_include" != check && test "$with_blas_include" != notset )
                   then
                      # --with-blas-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-library asking to check for blas but --with-blas-include has" >&5
$as_echo "$as_me: WARNING: --with-blas-library asking to check for blas but --with-blas-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-blas-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-blas-library ignored." >&2;}
                      use_blas=yes
                   fi
                fi
             else
                if ( test "$with_blas_library" != notset )
                then
                   # --with-blas-library a path or yes
                   if ( test "$with_blas_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-blas-library enabling blas but --with-blas-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_blas_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-include asking to check for blas but --with-blas-library has" >&5
$as_echo "$as_me: WARNING: --with-blas-include asking to check for blas but --with-blas-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-blas-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-blas-include ignored." >&2;}
                         use_blas=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-blas to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_blas != notset && test $with_blas != no && test $with_blas != check && test $with_blas != yes )
          then
             # We have a path to use
             if ( test -d $with_blas )
             then
                # Parameter is a directory path
                fallback_path=$with_blas
                trace 3 with-blas provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_blas ||
$as_expr X$with_blas : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_blas : 'X\(//\)[^/]' \| \
	 X$with_blas : 'X\(//\)$' \| \
	 X$with_blas : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_blas |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_blas" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_blas`
                   if ( test X$with_blas = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-blas provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-blas, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-blas, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-blas=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-blas, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-blas, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_blas != Xno )
          then
             if ( test $with_blas_pkgconf = no ||  test $with_blas_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-blas-library is a specification rather than a
                # simple switch
                if ( test "$with_blas_library" != notset && test "$with_blas_library" != no && test "$with_blas_library" != check && test "$with_blas_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_blas_lnklibs and include
                   # the initial part of the link line in with_blas_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_blas_lnklibs=""
                   with_blas_libpath=""
                   use_blas=yes
                   trace 6 Parsing --with-blas-library setting
                   with_blas_library=`echo " $with_blas_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_blas_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_blas_libpath="$with_blas_libpath$with_blas_lnklibs$part "
                              with_blas_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_blas_lnklibs="$with_blas_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_blas_libpath="$with_blas_libpath$with_blas_lnklibs$part "
                              with_blas_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_blas_libpath="$with_blas_libpath${with_blas_lnklibs}-L$part "
                                 with_blas_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_blas_libpath="$with_blas_libpath$with_blas_lnklibs-L$path "
                                          with_blas_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_blas_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-blas-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-blas-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-blas-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-blas-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_blas_lnklibs="${with_blas_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-blas-library is [$with_blas_libpath $with_blas_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_blas_libpath $with_blas_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_blas specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_blas_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_blas_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_blas_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-blas-library, using --with-blas value [$with_blas_lnklibs]
                      else
                         trace 3 No library name specified by --with-blas-library, using default ["-lblas"]
                         with_blas_lnklibs="-lblas"
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_blas specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_blas_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_blas_libpath="-L$fallback_path $with_blas_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-blas-library, using --with-blas to set library path for blas [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_blas_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-library and --with-blas specify different paths, --with-blas path ignored" >&5
$as_echo "$as_me: WARNING: --with-blas-library and --with-blas specify different paths, --with-blas path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_blas setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_blas_libpath $with_blas_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-library and --with-blas specify different libraries, --with-blas library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-blas-library and --with-blas specify different libraries, --with-blas library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_blas specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_blas_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-blas-library, using --with-blas value [-L$fallback_path]
                            with_blas_libpath="-L$fallback_path $with_blas_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_blas_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-library and --with-blas specify different paths, --with-blas path ignored" >&5
$as_echo "$as_me: WARNING: --with-blas-library and --with-blas specify different paths, --with-blas path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-blas-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-blas to set library path for blas [-L$fallback_path]
                      with_blas_libpath=-L$fallback_path
                      use_blas=yes
                   else
                      with_blas_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_blas specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_blas_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_blas_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-blas pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-blas pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_blas_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-blas to set library name for blas [$with_blas_lnklibs]
                   else
                      trace 3 Using default library name for blas ["-lblas"]
                      with_blas_lnklibs="-lblas"
                   fi
                fi


                # Check if --with-blas-include is a provided
                if ( test "$with_blas_include" != notset && test "$with_blas_include" != no && test "$with_blas_include" != check && test "$with_blas_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_blas_inc=""
                   use_blas=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-blas-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_blas_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for blas
                         with_blas_inc="$with_blas_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-blas-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-blas-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_blas_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-blas value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-blas value [-I$fallback_path]" >&2;}
                         with_blas_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_blas_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-blas-include and --with-blas specify different paths, --with-blas ignored" >&5
$as_echo "$as_me: WARNING: --with-blas-include and --with-blas specify different paths, --with-blas ignored" >&2;}
                      fi
                   fi
                else
                   # --with-blas-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-blas-include, using --with-blas value [-I$fallback_path]
                      with_blas_inc=-I$fallback_path
                      use_blas=yes
                   else
                      trace 3 Will use default system include path
                      with_blas_inc=""
                   fi
                fi
             fi # End of: if ( test $with_blas_pkgconf = no || notset
          else
             trace 4 Library blas disabled
          fi

          KnownLibs=$KnownLibs" "blas
          if ( test X$use_blas = X )
          then
             use_blas=check
             UnconfiguredLibs=$UnconfiguredLibs" "blas
          else
             ConfiguredLibs=$ConfiguredLibs" "blas
          fi

          with_blas_tstcd='sgemm_ ();'
                                                  with_blas_prq=""
          with_blas_hdr=""
          with_blas_lang="C
                             "
          with_blas_virtual="no"






