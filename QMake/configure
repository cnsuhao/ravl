$as_echo "$as_me: WARNING: --with-GLUT-include and --with-GLUT specify different paths, --with-GLUT ignored" >&2;}
                      fi
                   fi
                else
                   # --with-GLUT-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-GLUT-include, using --with-GLUT value [-I$fallback_path]
                      with_GLUT_inc=-I$fallback_path
                      use_GLUT=yes
                   else
                      trace 3 Will use default system include path
                      with_GLUT_inc=""
                   fi
                fi
             fi # End of: if ( test $with_GLUT_pkgconf = no || notset
          else
             trace 4 Library GLUT disabled
          fi

          KnownLibs=$KnownLibs" "GLUT
          if ( test X$use_GLUT = X )
          then
             use_GLUT=check
             UnconfiguredLibs=$UnconfiguredLibs" "GLUT
          else
             ConfiguredLibs=$ConfiguredLibs" "GLUT
          fi

          with_GLUT_tstcd='int *pargc;
                               char **argv;
                               glutInit(pargc, argv);
                              '
                                                  with_GLUT_prq=""
          with_GLUT_hdr="GL/glut.h"
          with_GLUT_lang="C
                             "
          with_GLUT_virtual="no"






