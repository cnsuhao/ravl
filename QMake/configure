                                    use_GLUT=yes
                                 else
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable GLUT via pkg-config but configure is unaware of the name of the DOTpc metadata file
See \`config.log' for more details" "$LINENO" 5; }
                                 fi
                                 ;;
                        'c' |'C' | 'ck' | 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 if ( test X != X )
                                 then
                                    trace 4 Will check for GLUT using --with-GLUT-pkgconf default value [$with_GLUT_pkgconf]
                                    with_GLUT_pkgconf=
                                    with_GLUT_check=pkgconfig
                                    with_GLUT_virtual=no
                                    use_GLUT=check
                                 else
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to check for GLUT via pkg-config but configure is unaware of the name of the DOTpc metadata file
See \`config.log' for more details" "$LINENO" 5; }
                                 fi
                                 ;;
                        * )
                                 trace 4 Asking for GLUT --with-GLUT-pkgconf set to [$with_GLUT_pkgconf]
                                 with_GLUT_check=pkgconfig
                                 with_GLUT_virtual=no
                                 use_GLUT=yes
                                 ;;
                       esac

else
  if ( test X != X )
                       then
                          trace 4 No --with-GLUT-pkgconf specified using default [$with_GLUT_pkgconf]
                          with_GLUT_pkgconf=
                          with_GLUT_check=pkgconfig
                          with_GLUT_virtual=no
                       else
                          trace 4 No --with-GLUT-pkgconf specified and no default value
                       fi


fi


