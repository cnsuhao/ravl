                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-libmkl-library and --with-libmkl specify different libraries, --with-libmkl library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-libmkl-library and --with-libmkl specify different libraries, --with-libmkl library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_libmkl specified a fallback path, are we using
                         # it?
