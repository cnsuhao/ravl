                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Qt3QGLViewer-library and --with-Qt3QGLViewer specify different paths, --with-Qt3QGLViewer path ignored" >&5
$as_echo "$as_me: WARNING: --with-Qt3QGLViewer-library and --with-Qt3QGLViewer specify different paths, --with-Qt3QGLViewer path ignored" >&2;}
