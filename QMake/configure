$as_echo "$as_me: WARNING: --with-Math-library asking to check for Math but --with-Math-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-Math-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-Math-library ignored." >&2;}
