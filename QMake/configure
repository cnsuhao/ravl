# Check whether --with-glib was given.
if test "${with_glib+set}" = set; then :
  withval=$with_glib; case $with_glib in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-glib disabling glib libraries
                                 if ( test X$use_glib = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable glib by --with-glib but --with-glib-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_glib = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling glib via --with-glib despite --with-glib-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling glib via --with-glib despite --with-glib-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_glib=no
                                 fi
                                 with_glib=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-glib enabling glib libraries
                                 if ( test X$use_glib = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable glib by --with-glib but --with-glib-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_glib = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling glib via --with-glib despite --with-glib-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling glib via --with-glib despite --with-glib-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_glib=yes
                                 fi
                                 with_glib=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-glib checking for glib libraries
                                 if (test X$use_glib = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-glib asking to check for the libs but --with-glib-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-glib asking to check for the libs but --with-glib-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_glib = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-glib asking to check for the libs but --with-glib-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-glib asking to check for the libs but --with-glib-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_glib=check
                                    fi
                                 fi
                                 with_glib=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_glib )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_glib
                                    fallback_file=""
                                    trace 3 with-glib provides a fallback path [$fallback_path]
                                    use_glib=yes
                                    with_glib=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_glib ||
$as_expr X$with_glib : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_glib : 'X\(//\)[^/]' \| \
	 X$with_glib : 'X\(//\)$' \| \
	 X$with_glib : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_glib |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_glib" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_glib`
                                       if ( test X$with_glib = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-glib provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_glib=yes
                                          with_glib=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-glib, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-glib, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-glib=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-glib, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-glib, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-glib specified

fi



          if ( test "$fallback_path/$fallback_file" != / )
          then
             # Do not currently support --with_<lib> specifying a .pc path
             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-glib set to an unknown option, setting ignored" >&5
