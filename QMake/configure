$as_echo "$as_me: WARNING: Explicitly asking for Zlib via --with-Zlib-library but --with-Zlib" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-Zlib ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-Zlib ignored." >&2;}
