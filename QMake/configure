             if ( test "$with_GLUT_include" != no && test "$with_GLUT_include" != notset )
             then
                if ( test "$with_GLUT_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT specifically disables library but --with-GLUT-include" >&5
$as_echo "$as_me: WARNING: --with-GLUT specifically disables library but --with-GLUT-include" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-GLUT-include ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-GLUT-include ignored" >&2;}
                   use_GLUT=no
                else
                   # --with-GLUT-include is yes or points to the include
                   # directory
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-include asking to use GLUT but --with-GLUT disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

          else
             # test --with-GLUT != no

             if ( test $with_GLUT = check )
             then
                if ( test $with_GLUT_pkgconf != no && test $with_GLUT_pkgconf != notset )
                then
                   # --with-GLUT-pkgconf must point to the metafile
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for GLUT via pkg-config but --with-GLUT is" >&5
$as_echo "$as_me: WARNING: Explicitly asking for GLUT via pkg-config but --with-GLUT is" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-GLUT ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-GLUT ignored." >&2;}
                   use_GLUT=yes
                fi

                if ( test "$with_GLUT_library" != check && test "$with_GLUT_library" != notset )
                then
                   if ( test "$with_GLUT_library" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for GLUT via --with-GLUT but --with-GLUT-library" >&5
$as_echo "$as_me: WARNING: Asking to check for GLUT via --with-GLUT but --with-GLUT-library" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-GLUT ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-GLUT ignored." >&2;}
                      use_GLUT=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for GLUT via --with-GLUT-library but --with-GLUT" >&5
$as_echo "$as_me: WARNING: Explicitly asking for GLUT via --with-GLUT-library but --with-GLUT" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-GLUT ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-GLUT ignored." >&2;}
                      use_GLUT=yes
                   fi
                fi

                if ( test "$with_GLUT_include" != check && test "$with_GLUT_include" != notset )
                then
                   if ( test "$with_GLUT_include" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for GLUT via --with-GLUT but --with-GLUT-include" >&5
$as_echo "$as_me: WARNING: Asking to check for GLUT via --with-GLUT but --with-GLUT-include" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-GLUT ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-GLUT ignored." >&2;}
                      use_GLUT=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for GLUT via --with-GLUT-include but --with-GLUT" >&5
$as_echo "$as_me: WARNING: Explicitly asking for GLUT via --with-GLUT-include but --with-GLUT" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-GLUT ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-GLUT ignored." >&2;}
                      use_GLUT=yes
                   fi
                fi

             else
                # --with-GLUT != no && != check
                if ( test $with_GLUT != notset )
                then
                   # --with-GLUT = path or yes

                   # We need not check --with-GLUT-pkgconf as there is no
                   # possible conflict (cannot be check and we take no to
                   # be a request to disable pkg-config, rather than the
                   # library as a whole)

                   if ( test "$with_GLUT_library" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-library disabling GLUT but --with-GLUT enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_GLUT_library" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling GLUT via --with-GLUT but --with-GLUT-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling GLUT via --with-GLUT but --with-GLUT-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-GLUT-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-GLUT-include ignored." >&2;}
                         use_GLUT=no
                      fi
                   fi

                   if ( test "$with_GLUT_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-include disabling GLUT but --with-GLUT enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_GLUT_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling GLUT via --with-GLUT but --with-GLUT-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling GLUT via --with-GLUT but --with-GLUT-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-GLUT-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-GLUT-include ignored." >&2;}
                         use_GLUT=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-GLUT-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_GLUT_pkgconf != notset && test $with_GLUT_pkgconf != no )
          then
             # --with-GLUT-pkgconf = path (cannot be yes or check)
             if ( test "$with_GLUT_library" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-library asking to check for GLUT but --with-GLUT-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-GLUT-library asking to check for GLUT but --with-GLUT-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-GLUT-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-GLUT-library ignored." >&2;}
                use_GLUT=yes
             else
                if ( test "$with_GLUT_library" != no && test "$with_GLUT_library" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-library enabling compile check for GLUT but --with-GLUT-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test "$with_GLUT_include" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-include asking to check for GLUT but --with-GLUT-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-GLUT-include asking to check for GLUT but --with-GLUT-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-GLUT-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-GLUT-include ignored." >&2;}
                use_GLUT=yes
             else
                if ( test "$with_GLUT_include" != no && test "$with_GLUT_include" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-include enabling compile check for GLUT but --with-GLUT-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test "$with_GLUT_library" = no )
          then
             if ( test "$with_GLUT_include" != no && test "$with_GLUT_include" != notset )
             then
                if ( test "$with_GLUT_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-include asking to check for GLUT but --with-GLUT-library has" >&5
$as_echo "$as_me: WARNING: --with-GLUT-include asking to check for GLUT but --with-GLUT-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-GLUT-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-GLUT-include ignored." >&2;}
                   use_GLUT=no
                else
                   # --with-GLUT-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-include enabling GLUT but --with-GLUT-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-GLUT-library != no
             if ( test "$with_GLUT_library" = check )
             then
                if ( test "$with_GLUT_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-library asking to check for GLUT but --with-GLUT-include has" >&5
$as_echo "$as_me: WARNING: --with-GLUT-library asking to check for GLUT but --with-GLUT-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-GLUT-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-GLUT-library ignored." >&2;}
                   use_GLUT=no
                else
                   if ( test "$with_GLUT_include" != check && test "$with_GLUT_include" != notset )
                   then
                      # --with-GLUT-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-library asking to check for GLUT but --with-GLUT-include has" >&5
$as_echo "$as_me: WARNING: --with-GLUT-library asking to check for GLUT but --with-GLUT-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-GLUT-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-GLUT-library ignored." >&2;}
                      use_GLUT=yes
                   fi
                fi
             else
                if ( test "$with_GLUT_library" != notset )
                then
                   # --with-GLUT-library a path or yes
                   if ( test "$with_GLUT_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-GLUT-library enabling GLUT but --with-GLUT-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_GLUT_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-include asking to check for GLUT but --with-GLUT-library has" >&5
$as_echo "$as_me: WARNING: --with-GLUT-include asking to check for GLUT but --with-GLUT-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-GLUT-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-GLUT-include ignored." >&2;}
                         use_GLUT=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-GLUT to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_GLUT != notset && test $with_GLUT != no && test $with_GLUT != check && test $with_GLUT != yes )
          then
             # We have a path to use
             if ( test -d $with_GLUT )
             then
                # Parameter is a directory path
                fallback_path=$with_GLUT
                trace 3 with-GLUT provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_GLUT ||
$as_expr X$with_GLUT : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_GLUT : 'X\(//\)[^/]' \| \
	 X$with_GLUT : 'X\(//\)$' \| \
	 X$with_GLUT : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_GLUT |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_GLUT" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_GLUT`
                   if ( test X$with_GLUT = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-GLUT provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-GLUT, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-GLUT, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-GLUT=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-GLUT, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-GLUT, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_GLUT != Xno )
          then
             if ( test $with_GLUT_pkgconf = no ||  test $with_GLUT_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-GLUT-library is a specification rather than a
                # simple switch
                if ( test "$with_GLUT_library" != notset && test "$with_GLUT_library" != no && test "$with_GLUT_library" != check && test "$with_GLUT_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_GLUT_lnklibs and include
                   # the initial part of the link line in with_GLUT_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_GLUT_lnklibs=""
                   with_GLUT_libpath=""
                   use_GLUT=yes
                   trace 6 Parsing --with-GLUT-library setting
                   with_GLUT_library=`echo " $with_GLUT_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_GLUT_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_GLUT_libpath="$with_GLUT_libpath$with_GLUT_lnklibs$part "
                              with_GLUT_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_GLUT_lnklibs="$with_GLUT_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_GLUT_libpath="$with_GLUT_libpath$with_GLUT_lnklibs$part "
                              with_GLUT_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_GLUT_libpath="$with_GLUT_libpath${with_GLUT_lnklibs}-L$part "
                                 with_GLUT_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_GLUT_libpath="$with_GLUT_libpath$with_GLUT_lnklibs-L$path "
                                          with_GLUT_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_GLUT_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-GLUT-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-GLUT-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-GLUT-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-GLUT-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_GLUT_lnklibs="${with_GLUT_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-GLUT-library is [$with_GLUT_libpath $with_GLUT_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_GLUT_libpath $with_GLUT_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_GLUT specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_GLUT_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_GLUT_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-GLUT pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-GLUT pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_GLUT_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-GLUT-library, using --with-GLUT value [$with_GLUT_lnklibs]
                      else
                         trace 3 No library name specified by --with-GLUT-library, using default ["-lglut"]
                         with_GLUT_lnklibs="-lglut"
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_GLUT specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_GLUT_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_GLUT_libpath="-L$fallback_path $with_GLUT_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-GLUT-library, using --with-GLUT to set library path for GLUT [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_GLUT_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-library and --with-GLUT specify different paths, --with-GLUT path ignored" >&5
$as_echo "$as_me: WARNING: --with-GLUT-library and --with-GLUT specify different paths, --with-GLUT path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_GLUT setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_GLUT_libpath $with_GLUT_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-library and --with-GLUT specify different libraries, --with-GLUT library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-GLUT-library and --with-GLUT specify different libraries, --with-GLUT library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_GLUT specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_GLUT_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-GLUT-library, using --with-GLUT value [-L$fallback_path]
                            with_GLUT_libpath="-L$fallback_path $with_GLUT_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_GLUT_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT-library and --with-GLUT specify different paths, --with-GLUT path ignored" >&5
$as_echo "$as_me: WARNING: --with-GLUT-library and --with-GLUT specify different paths, --with-GLUT path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-GLUT-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-GLUT to set library path for GLUT [-L$fallback_path]
                      with_GLUT_libpath=-L$fallback_path
                      use_GLUT=yes
                   else
                      with_GLUT_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_GLUT specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
