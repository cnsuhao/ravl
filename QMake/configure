                                    if ( test X$use_CXSparse = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling CXSparse via --with-CXSparse despite --with-CXSparse-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling CXSparse via --with-CXSparse despite --with-CXSparse-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_CXSparse=yes
                                 fi
                                 with_CXSparse=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-CXSparse checking for CXSparse libraries
                                 if (test X$use_CXSparse = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-CXSparse asking to check for the libs but --with-CXSparse-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-CXSparse asking to check for the libs but --with-CXSparse-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_CXSparse = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-CXSparse asking to check for the libs but --with-CXSparse-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-CXSparse asking to check for the libs but --with-CXSparse-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_CXSparse=check
                                    fi
                                 fi
                                 with_CXSparse=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_CXSparse )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_CXSparse
                                    fallback_file=""
                                    trace 3 with-CXSparse provides a fallback path [$fallback_path]
                                    use_CXSparse=yes
                                    with_CXSparse=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_CXSparse ||
$as_expr X$with_CXSparse : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_CXSparse : 'X\(//\)[^/]' \| \
	 X$with_CXSparse : 'X\(//\)$' \| \
	 X$with_CXSparse : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_CXSparse |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_CXSparse" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_CXSparse`
                                       if ( test X$with_CXSparse = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-CXSparse provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_CXSparse=yes
                                          with_CXSparse=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-CXSparse, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-CXSparse, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-CXSparse=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-CXSparse, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-CXSparse, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-CXSparse specified

fi






