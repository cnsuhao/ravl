                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-library and --with-vtkFiltering specify different libraries, --with-vtkFiltering library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-library and --with-vtkFiltering specify different libraries, --with-vtkFiltering library setting ignored" >&2;}
