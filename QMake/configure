                   if ( test "$with_vtkFiltering_library" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vtkFiltering-library disabling vtkFiltering but --with-vtkFiltering enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_vtkFiltering_library" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling vtkFiltering via --with-vtkFiltering but --with-vtkFiltering-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling vtkFiltering via --with-vtkFiltering but --with-vtkFiltering-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-vtkFiltering-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-vtkFiltering-include ignored." >&2;}
                         use_vtkFiltering=no
                      fi
                   fi

                   if ( test "$with_vtkFiltering_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vtkFiltering-include disabling vtkFiltering but --with-vtkFiltering enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_vtkFiltering_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling vtkFiltering via --with-vtkFiltering but --with-vtkFiltering-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling vtkFiltering via --with-vtkFiltering but --with-vtkFiltering-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-vtkFiltering-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-vtkFiltering-include ignored." >&2;}
                         use_vtkFiltering=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-vtkFiltering-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_vtkFiltering_pkgconf != notset && test $with_vtkFiltering_pkgconf != no )
          then
             # --with-vtkFiltering-pkgconf = path (cannot be yes or check)
             if ( test "$with_vtkFiltering_library" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-library asking to check for vtkFiltering but --with-vtkFiltering-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-library asking to check for vtkFiltering but --with-vtkFiltering-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-vtkFiltering-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-vtkFiltering-library ignored." >&2;}
                use_vtkFiltering=yes
             else
                if ( test "$with_vtkFiltering_library" != no && test "$with_vtkFiltering_library" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vtkFiltering-library enabling compile check for vtkFiltering but --with-vtkFiltering-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test "$with_vtkFiltering_include" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-include asking to check for vtkFiltering but --with-vtkFiltering-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-include asking to check for vtkFiltering but --with-vtkFiltering-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-vtkFiltering-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-vtkFiltering-include ignored." >&2;}
                use_vtkFiltering=yes
             else
                if ( test "$with_vtkFiltering_include" != no && test "$with_vtkFiltering_include" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vtkFiltering-include enabling compile check for vtkFiltering but --with-vtkFiltering-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test "$with_vtkFiltering_library" = no )
          then
             if ( test "$with_vtkFiltering_include" != no && test "$with_vtkFiltering_include" != notset )
             then
                if ( test "$with_vtkFiltering_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-include asking to check for vtkFiltering but --with-vtkFiltering-library has" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-include asking to check for vtkFiltering but --with-vtkFiltering-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-vtkFiltering-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-vtkFiltering-include ignored." >&2;}
                   use_vtkFiltering=no
                else
                   # --with-vtkFiltering-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vtkFiltering-include enabling vtkFiltering but --with-vtkFiltering-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-vtkFiltering-library != no
             if ( test "$with_vtkFiltering_library" = check )
             then
                if ( test "$with_vtkFiltering_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-library asking to check for vtkFiltering but --with-vtkFiltering-include has" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-library asking to check for vtkFiltering but --with-vtkFiltering-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-vtkFiltering-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-vtkFiltering-library ignored." >&2;}
                   use_vtkFiltering=no
                else
                   if ( test "$with_vtkFiltering_include" != check && test "$with_vtkFiltering_include" != notset )
                   then
                      # --with-vtkFiltering-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-library asking to check for vtkFiltering but --with-vtkFiltering-include has" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-library asking to check for vtkFiltering but --with-vtkFiltering-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-vtkFiltering-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-vtkFiltering-library ignored." >&2;}
                      use_vtkFiltering=yes
                   fi
                fi
             else
                if ( test "$with_vtkFiltering_library" != notset )
                then
                   # --with-vtkFiltering-library a path or yes
                   if ( test "$with_vtkFiltering_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vtkFiltering-library enabling vtkFiltering but --with-vtkFiltering-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_vtkFiltering_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-include asking to check for vtkFiltering but --with-vtkFiltering-library has" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-include asking to check for vtkFiltering but --with-vtkFiltering-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-vtkFiltering-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-vtkFiltering-include ignored." >&2;}
                         use_vtkFiltering=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-vtkFiltering to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_vtkFiltering != notset && test $with_vtkFiltering != no && test $with_vtkFiltering != check && test $with_vtkFiltering != yes )
          then
             # We have a path to use
             if ( test -d $with_vtkFiltering )
             then
                # Parameter is a directory path
                fallback_path=$with_vtkFiltering
                trace 3 with-vtkFiltering provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_vtkFiltering ||
$as_expr X$with_vtkFiltering : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_vtkFiltering : 'X\(//\)[^/]' \| \
	 X$with_vtkFiltering : 'X\(//\)$' \| \
	 X$with_vtkFiltering : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_vtkFiltering |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_vtkFiltering" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_vtkFiltering`
                   if ( test X$with_vtkFiltering = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-vtkFiltering provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-vtkFiltering, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-vtkFiltering, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-vtkFiltering=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-vtkFiltering, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-vtkFiltering, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_vtkFiltering != Xno )
          then
             if ( test $with_vtkFiltering_pkgconf = no ||  test $with_vtkFiltering_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-vtkFiltering-library is a specification rather than a
                # simple switch
                if ( test "$with_vtkFiltering_library" != notset && test "$with_vtkFiltering_library" != no && test "$with_vtkFiltering_library" != check && test "$with_vtkFiltering_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_vtkFiltering_lnklibs and include
                   # the initial part of the link line in with_vtkFiltering_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_vtkFiltering_lnklibs=""
                   with_vtkFiltering_libpath=""
                   use_vtkFiltering=yes
                   trace 6 Parsing --with-vtkFiltering-library setting
                   with_vtkFiltering_library=`echo " $with_vtkFiltering_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_vtkFiltering_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_vtkFiltering_libpath="$with_vtkFiltering_libpath$with_vtkFiltering_lnklibs$part "
                              with_vtkFiltering_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_vtkFiltering_lnklibs="$with_vtkFiltering_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_vtkFiltering_libpath="$with_vtkFiltering_libpath$with_vtkFiltering_lnklibs$part "
                              with_vtkFiltering_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_vtkFiltering_libpath="$with_vtkFiltering_libpath${with_vtkFiltering_lnklibs}-L$part "
                                 with_vtkFiltering_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkFiltering_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkFiltering_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkFiltering_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkFiltering_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_vtkFiltering_libpath="$with_vtkFiltering_libpath$with_vtkFiltering_lnklibs-L$path "
                                          with_vtkFiltering_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_vtkFiltering_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-vtkFiltering-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-vtkFiltering-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-vtkFiltering-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-vtkFiltering-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkFiltering_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkFiltering_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkFiltering_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkFiltering_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_vtkFiltering_lnklibs="${with_vtkFiltering_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-vtkFiltering-library is [$with_vtkFiltering_libpath $with_vtkFiltering_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_vtkFiltering_libpath $with_vtkFiltering_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_vtkFiltering specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_vtkFiltering_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkFiltering pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkFiltering pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_vtkFiltering_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkFiltering pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkFiltering pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_vtkFiltering_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-vtkFiltering-library, using --with-vtkFiltering value [$with_vtkFiltering_lnklibs]
                      else
