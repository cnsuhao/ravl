          if ( test "$with_tetgen_library" = no )
          then
             if ( test "$with_tetgen_include" != no && test "$with_tetgen_include" != notset )
             then
                if ( test "$with_tetgen_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-include asking to check for tetgen but --with-tetgen-library has" >&5
$as_echo "$as_me: WARNING: --with-tetgen-include asking to check for tetgen but --with-tetgen-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-tetgen-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-tetgen-include ignored." >&2;}
                   use_tetgen=no
                else
                   # --with-tetgen-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-tetgen-include enabling tetgen but --with-tetgen-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-tetgen-library != no
             if ( test "$with_tetgen_library" = check )
             then
                if ( test "$with_tetgen_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library asking to check for tetgen but --with-tetgen-include has" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library asking to check for tetgen but --with-tetgen-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-tetgen-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-tetgen-library ignored." >&2;}
                   use_tetgen=no
                else
                   if ( test "$with_tetgen_include" != check && test "$with_tetgen_include" != notset )
                   then
                      # --with-tetgen-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library asking to check for tetgen but --with-tetgen-include has" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library asking to check for tetgen but --with-tetgen-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-tetgen-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-tetgen-library ignored." >&2;}
                      use_tetgen=yes
                   fi
                fi
             else
                if ( test "$with_tetgen_library" != notset )
                then
                   # --with-tetgen-library a path or yes
                   if ( test "$with_tetgen_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-tetgen-library enabling tetgen but --with-tetgen-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_tetgen_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-include asking to check for tetgen but --with-tetgen-library has" >&5
$as_echo "$as_me: WARNING: --with-tetgen-include asking to check for tetgen but --with-tetgen-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-tetgen-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-tetgen-include ignored." >&2;}
                         use_tetgen=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-tetgen to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_tetgen != notset && test $with_tetgen != no && test $with_tetgen != check && test $with_tetgen != yes )
          then
             # We have a path to use
             if ( test -d $with_tetgen )
             then
                # Parameter is a directory path
                fallback_path=$with_tetgen
                trace 3 with-tetgen provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_tetgen ||
$as_expr X$with_tetgen : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_tetgen : 'X\(//\)[^/]' \| \
	 X$with_tetgen : 'X\(//\)$' \| \
	 X$with_tetgen : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_tetgen |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_tetgen" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_tetgen`
                   if ( test X$with_tetgen = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-tetgen provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-tetgen, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-tetgen, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-tetgen=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-tetgen, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-tetgen, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_tetgen != Xno )
          then
             if ( test $with_tetgen_pkgconf = no ||  test $with_tetgen_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-tetgen-library is a specification rather than a
                # simple switch
                if ( test "$with_tetgen_library" != notset && test "$with_tetgen_library" != no && test "$with_tetgen_library" != check && test "$with_tetgen_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_tetgen_lnklibs and include
                   # the initial part of the link line in with_tetgen_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_tetgen_lnklibs=""
                   with_tetgen_libpath=""
                   use_tetgen=yes
                   trace 6 Parsing --with-tetgen-library setting
                   with_tetgen_library=`echo " $with_tetgen_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_tetgen_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_tetgen_libpath="$with_tetgen_libpath$with_tetgen_lnklibs$part "
                              with_tetgen_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_tetgen_lnklibs="$with_tetgen_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_tetgen_libpath="$with_tetgen_libpath$with_tetgen_lnklibs$part "
                              with_tetgen_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_tetgen_libpath="$with_tetgen_libpath${with_tetgen_lnklibs}-L$part "
                                 with_tetgen_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_tetgen_libpath="$with_tetgen_libpath$with_tetgen_lnklibs-L$path "
                                          with_tetgen_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_tetgen_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-tetgen-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-tetgen-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-tetgen-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-tetgen-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_tetgen_lnklibs="${with_tetgen_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-tetgen-library is [$with_tetgen_libpath $with_tetgen_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_tetgen_libpath $with_tetgen_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_tetgen specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_tetgen_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_tetgen_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_tetgen_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-tetgen-library, using --with-tetgen value [$with_tetgen_lnklibs]
                      else
                         trace 3 No library name specified by --with-tetgen-library, using default ["-ltet"]
                         with_tetgen_lnklibs="-ltet"
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_tetgen specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_tetgen_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_tetgen_libpath="-L$fallback_path $with_tetgen_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-tetgen-library, using --with-tetgen to set library path for tetgen [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_tetgen_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_tetgen setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_tetgen_libpath $with_tetgen_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library and --with-tetgen specify different libraries, --with-tetgen library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library and --with-tetgen specify different libraries, --with-tetgen library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_tetgen specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_tetgen_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-tetgen-library, using --with-tetgen value [-L$fallback_path]
                            with_tetgen_libpath="-L$fallback_path $with_tetgen_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_tetgen_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&5
$as_echo "$as_me: WARNING: --with-tetgen-library and --with-tetgen specify different paths, --with-tetgen path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-tetgen-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-tetgen to set library path for tetgen [-L$fallback_path]
                      with_tetgen_libpath=-L$fallback_path
                      use_tetgen=yes
                   else
                      with_tetgen_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_tetgen specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_tetgen_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_tetgen_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen pointing to a .so" >&2;}
