                         if ( test X`$EXPR "X $with_vlfeat_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-vlfeat-library, using --with-vlfeat value [-L$fallback_path]
                            with_vlfeat_libpath="-L$fallback_path $with_vlfeat_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_vlfeat_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-library and --with-vlfeat specify different paths, --with-vlfeat path ignored" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-library and --with-vlfeat specify different paths, --with-vlfeat path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-vlfeat-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-vlfeat to set library path for vlfeat [-L$fallback_path]
                      with_vlfeat_libpath=-L$fallback_path
                      use_vlfeat=yes
                   else
                      with_vlfeat_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
