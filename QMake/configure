                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-library has specifically enabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-library has specifically enabled it" >&2;}
                                       else
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF has specifically enabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF has specifically enabled it" >&2;}
                                       fi
                                    fi
                                 else
                                    if ( test X$use_LibTIFF = Xno )
                                    then
                                       if ( test X$with_LibTIFF_pkgconf = Xno )
                                       then
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-pkgconf has specifically disabled it" >&2;}
                                       else
                                          if ( test X$with_LibTIFF_library = Xno )
                                          then
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-library has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-library has specifically disabled it" >&2;}
                                          else
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for LibTIFF via --with-LibTIFF-include but --with-LibTIFF has specifically disabled it" >&2;}
                                          fi
                                       fi
                                    else
                                       if ( test X$use_LibTIFF = Xcheck )
                                       then
                                          # Can never be here because of
                                          # --with-<lib>-pkgconf so we do not
                                          # need to check for any incompatable
                                          # settings
                                          with_LibTIFF_include=check
                                       else
                                          # No other user setting
                                          use_LibTIFF=check
                                          # This is the only condition where
                                          # we need to be updating use_<lib>
                                       fi
                                    fi
                                 fi
                                 with_LibTIFF_include=check
                                 ;;
                          * )
                                 if ( test X$use_LibTIFF = Xyes )
                                 then
                                    if ( test X$with_LibTIFF_check = Xpkgconfig )
                                    then
                                       # Either --with-<lib>-pkgconf is in use
                                       # or --with-<lib> specifies a .pc file
                                       { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for LibTIFF via --with-LibTIFF-include but another configuration option has specifically asked to use pkg-config
See \`config.log' for more details" "$LINENO" 5; }
                                    fi
                                 else
                                    if ( test X$use_LibTIFF = Xno )
                                    then
                                       if ( test X$with_LibTIFF_pkgconf = Xno )
                                       then
                                          { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-pkgconf has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                       else
                                          if ( test X$with_LibTIFF_library = Xno )
                                          then
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for LibTIFF via --with-LibTIFF-include but --with-LibTIFF-library has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          else
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for LibTIFF via --with-LibTIFF-include but --with-LibTIFF has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          fi
                                       fi
                                    else
                                       if ( test X$use_LibTIFF = Xcheck )
                                       then
                                          # Can never be here because of
                                          # --with-<lib>-pkgconf
                                          if ( test X$with_LibTIFF_library = Xcheck )
                                          then
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to use specific header files for LibTIFF via --with-LibTIFF-include despite --with-LibTIFF-library having asked to check for the package" >&5
$as_echo "$as_me: WARNING: Asking to use specific header files for LibTIFF via --with-LibTIFF-include despite --with-LibTIFF-library having asked to check for the package" >&2;}
                                          else
