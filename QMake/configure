                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-vtkFiltering value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-vtkFiltering value [-I$fallback_path]" >&2;}
                         with_vtkFiltering_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
