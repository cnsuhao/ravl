	 X$with_Qt5Sql : 'X\(//\)$' \| \
	 X$with_Qt5Sql : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Qt5Sql |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_Qt5Sql" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_Qt5Sql`
                                       if ( test X$with_Qt5Sql = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-Qt5Sql provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_Qt5Sql=yes
                                          with_Qt5Sql=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Qt5Sql, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Qt5Sql, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-Qt5Sql=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Qt5Sql, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Qt5Sql, parameter ignored" >&2;}
