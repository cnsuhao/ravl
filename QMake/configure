                      # --with_vtkGraphics setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_vtkGraphics_libpath $with_vtkGraphics_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkGraphics-library and --with-vtkGraphics specify different libraries, --with-vtkGraphics library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-vtkGraphics-library and --with-vtkGraphics specify different libraries, --with-vtkGraphics library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_vtkGraphics specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_vtkGraphics_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-vtkGraphics-library, using --with-vtkGraphics value [-L$fallback_path]
                            with_vtkGraphics_libpath="-L$fallback_path $with_vtkGraphics_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_vtkGraphics_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkGraphics-library and --with-vtkGraphics specify different paths, --with-vtkGraphics path ignored" >&5
$as_echo "$as_me: WARNING: --with-vtkGraphics-library and --with-vtkGraphics specify different paths, --with-vtkGraphics path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-vtkGraphics-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-vtkGraphics to set library path for vtkGraphics [-L$fallback_path]
                      with_vtkGraphics_libpath=-L$fallback_path
                      use_vtkGraphics=yes
                   else
                      with_vtkGraphics_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_vtkGraphics specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_vtkGraphics_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkGraphics pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkGraphics pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_vtkGraphics_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vtkGraphics pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-vtkGraphics pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_vtkGraphics_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-vtkGraphics to set library name for vtkGraphics [$with_vtkGraphics_lnklibs]
                   else
                      trace 3 Using default library name for vtkGraphics ["-lvtkGraphics -lvtkverdict"]
                      with_vtkGraphics_lnklibs="-lvtkGraphics -lvtkverdict"
                   fi
                fi


                # Check if --with-vtkGraphics-include is a path parameter
                if ( test "$with_vtkGraphics_include" != notset && test "$with_vtkGraphics_include" != no && test "$with_vtkGraphics_include" != check && test "$with_vtkGraphics_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_vtkGraphics_inc=""
                   use_vtkGraphics=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-vtkGraphics-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_vtkGraphics_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for vtkGraphics
                         with_vtkGraphics_inc="$with_vtkGraphics_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-vtkGraphics-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-vtkGraphics-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_vtkGraphics_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-vtkGraphics value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-vtkGraphics value [-I$fallback_path]" >&2;}
                         with_vtkGraphics_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_vtkGraphics_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkGraphics-include and --with-vtkGraphics specify different paths, --with-vtkGraphics ignored" >&5
$as_echo "$as_me: WARNING: --with-vtkGraphics-include and --with-vtkGraphics specify different paths, --with-vtkGraphics ignored" >&2;}
                      fi
                   fi
                else
                   # --with-vtkGraphics-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-vtkGraphics-include, using --with-vtkGraphics value [-I$fallback_path]
                      with_vtkGraphics_inc=-I$fallback_path
                      use_vtkGraphics=yes
                   else
                      trace 3 Will use default system include path
                      with_vtkGraphics_inc=""
                   fi
                fi
             fi # End of: if ( test $with_vtkGraphics_check !- pkgconfig
          else
             trace 4 Library vtkGraphics disabled
          fi



          with_vtkGraphics_tstcd='vtkDecimatePro *decimatePro = vtkDecimatePro::New();'
                                                  with_vtkGraphics_prq="vtkFiltering"
          with_vtkGraphics_hdr="vtkDecimatePro.h"
          with_vtkGraphics_lang="C++
                             "





          KnownLibs=$KnownLibs" "vtkGraphics
          if ( test X$use_vtkGraphics = X )
          then
             use_vtkGraphics=check
             UnconfiguredLibs=$UnconfiguredLibs" "vtkGraphics
          else
             ConfiguredLibs=$ConfiguredLibs" "vtkGraphics
          fi







          with_Yajl_check=compile
          if ( test Xcompile = Xvirtual )
          then
             with_Yajl_virtual=yes
          else
             with_Yajl_virtual=no
          fi

          fallback_file=""
          fallback_path=""


