                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-Math-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-Math-include ignored." >&2;}
                   use_Math=no
                else
                   # --with-Math-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Math-include enabling Math but --with-Math-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
