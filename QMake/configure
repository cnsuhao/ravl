$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for JPEG via --with-JPEG-include but --with-JPEG has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
