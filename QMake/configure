                                          use_UMFPack=yes
                                          with_UMFPack_include=yes
                                          ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                          use_UMFPack=check
                                          with_UMFPack_include=check
                                          ;;
                        * )               use_UMFPack=yes
                                          ;;
                       esac

else
  trace 4 No --with-UMFPack-include specified
                       with_UMFPack_include=notset


fi

          # Having read the command-line options, check none are mutually
          # exclusive or contradictory
          #
          # First test --with-UMFPack compatability with the rest
          if ( test $with_UMFPack = no )
          then
             if ( test $with_UMFPack_pkgconf != no && test $with_UMFPack_pkgconf != notset )
             then
                # --with-UMFPack-pkgconf=path (is neither no or notset and cannot
                # be yes or check as they would have errored above.
                { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "Asking to UMFPack via pkg-config but --with-UMFPack disabled it
See \`config.log' for more details." "$LINENO" 5; }
             fi

             if ( test "$with_UMFPack_library" != no && test "$with_UMFPack_library" != notset )
