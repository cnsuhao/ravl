                                       then
                                          { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for JPEG via --with-JPEG-include but --with-JPEG-pkgconf has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                       else
                                          if ( test X$with_JPEG_library = Xno )
                                          then
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for JPEG via --with-JPEG-include but --with-JPEG-library has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          else
