                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-libmkl-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-libmkl-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_libmkl_inc == X )
                   then
