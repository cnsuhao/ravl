                         trace 3 No library name specified by --with-Math-library, using --with-Math value [$with_Math_lnklibs]
                      else
                         trace 3 No library name specified by --with-Math-library, using default ["-lm"]
                         with_Math_lnklibs="-lm"
                      fi

                      if ( test X$fallback_path != X )
                      then
