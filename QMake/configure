# Check whether --with-OpenGL was given.
if test "${with_OpenGL+set}" = set; then :
  withval=$with_OpenGL; case $with_OpenGL in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-OpenGL disabling OpenGL libraries
                                 if ( test X$use_OpenGL = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable OpenGL by --with-OpenGL but --with-OpenGL-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_OpenGL = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling OpenGL via --with-OpenGL despite --with-OpenGL-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling OpenGL via --with-OpenGL despite --with-OpenGL-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_OpenGL=no
                                 fi
                                 with_OpenGL=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-OpenGL enabling OpenGL libraries
                                 if ( test X$use_OpenGL = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable OpenGL by --with-OpenGL but --with-OpenGL-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_OpenGL = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling OpenGL via --with-OpenGL despite --with-OpenGL-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling OpenGL via --with-OpenGL despite --with-OpenGL-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_OpenGL=yes
                                 fi
                                 with_OpenGL=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-OpenGL checking for OpenGL libraries
                                 if (test X$use_OpenGL = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-OpenGL asking to check for the libs but --with-OpenGL-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-OpenGL asking to check for the libs but --with-OpenGL-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_OpenGL = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-OpenGL asking to check for the libs but --with-OpenGL-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-OpenGL asking to check for the libs but --with-OpenGL-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_OpenGL=check
                                    fi
                                 fi
                                 with_OpenGL=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_OpenGL )
                                 then
                                    # Parameter is a directory path
