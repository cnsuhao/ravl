                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-library and --with-Socket specify different paths, --with-Socket path ignored" >&5
$as_echo "$as_me: WARNING: --with-Socket-library and --with-Socket specify different paths, --with-Socket path ignored" >&2;}
