                                          use_UMFPack=yes
                                          with_UMFPack=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-UMFPack, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-UMFPack, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
