$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to check for OpenGL via pkg-config but configure is unaware of the name of the DOTpc metadata file
See \`config.log' for more details" "$LINENO" 5; }
                                 fi
                                 ;;
                        * )
                                 trace 4 Asking for OpenGL --with-OpenGL-pkgconf set to [$with_OpenGL_pkgconf]
                                 with_OpenGL_check=pkgconfig
                                 with_OpenGL_virtual=no
                                 use_OpenGL=yes
                                 ;;
                       esac

else
  if ( test X != X )
                       then
                          trace 4 No --with-OpenGL-pkgconf specified using default [$with_OpenGL_pkgconf]
                          with_OpenGL_pkgconf=
                          with_OpenGL_check=pkgconfig
                          with_OpenGL_virtual=no
                       else
