                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-QtGui asking to check for the libs but --with-QtGui-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-QtGui asking to check for the libs but --with-QtGui-pkgconf has specifically disabled it" >&2;}
