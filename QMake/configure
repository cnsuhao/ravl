                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-CUDA, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-CUDA, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-CUDA specified

