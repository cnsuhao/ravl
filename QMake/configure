                      trace 3 Using path component of --with-vtkFiltering to set library path for vtkFiltering [-L$fallback_path]
                      with_vtkFiltering_libpath=-L$fallback_path
                      use_vtkFiltering=yes
                   else
                      with_vtkFiltering_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_vtkFiltering specified a fallback name, use that but
