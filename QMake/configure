if test "${with_QtGui+set}" = set; then :
  withval=$with_QtGui; case $with_QtGui in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-QtGui disabling QtGui libraries
                                 if ( test X$use_QtGui = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable QtGui by --with-QtGui but --with-QtGui-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_QtGui = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling QtGui via --with-QtGui despite --with-QtGui-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling QtGui via --with-QtGui despite --with-QtGui-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_QtGui=no
                                 fi
                                 with_QtGui=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-QtGui enabling QtGui libraries
                                 if ( test X$use_QtGui = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable QtGui by --with-QtGui but --with-QtGui-pkgconf has specifically disabled it
