                         trace 3 Using file component of --with-vlfeat to set library name for vlfeat [$with_vlfeat_lnklibs]
                   else
                      trace 3 Using default library name for vlfeat ["-lvl"]
                      with_vlfeat_lnklibs="-lvl"
                   fi
                fi


                # Check if --with-vlfeat-include is a provided
                if ( test "$with_vlfeat_include" != notset && test "$with_vlfeat_include" != no && test "$with_vlfeat_include" != check && test "$with_vlfeat_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_vlfeat_inc=""
                   use_vlfeat=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-vlfeat-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_vlfeat_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for vlfeat
                         with_vlfeat_inc="$with_vlfeat_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-vlfeat-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-vlfeat-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_vlfeat_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-vlfeat value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-vlfeat value [-I$fallback_path]" >&2;}
                         with_vlfeat_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_vlfeat_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
