                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Math-library and --with-Math specify different libraries, --with-Math library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-Math-library and --with-Math specify different libraries, --with-Math library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_Math specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_Math_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-Math-library, using --with-Math value [-L$fallback_path]
