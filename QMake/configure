                                 else
                                    if ( test X$use_Math = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Math asking to check for the libs but --with-Math-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-Math asking to check for the libs but --with-Math-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_Math=check
                                    fi
                                 fi
                                 with_Math=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_Math )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_Math
                                    fallback_file=""
                                    trace 3 with-Math provides a fallback path [$fallback_path]
                                    use_Math=yes
                                    with_Math=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_Math ||
$as_expr X$with_Math : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Math : 'X\(//\)[^/]' \| \
	 X$with_Math : 'X\(//\)$' \| \
	 X$with_Math : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Math |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_Math" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_Math`
                                       if ( test X$with_Math = X$fallback_path/$fallback_file )
                                       then
