$as_echo "$as_me: WARNING: Explicitly asking for vtkFiltering via --with-vtkFiltering-include but --with-vtkFiltering" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-vtkFiltering ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-vtkFiltering ignored." >&2;}
                      use_vtkFiltering=yes
