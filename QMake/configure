                                                    LeftToDo=`echo " $LeftToDo " | $SED "s/ $lib / /"`
                                                    RecentlyProcessed="$RecentlyProcessed $lib"
                                                    process_now=no
                                                    break
                                                    ;;
                                           needed ) { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "Library $lib is a prerequisite of a selected library but is unavailable due to a indeterminate pre-requisite state
See \`config.log' for more details." "$LINENO" 5; }
                                                    ;;
                                           yes )    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "Required library $lib is unavailable due to a indeterminate pre-requisite state
See \`config.log' for more details." "$LINENO" 5; }
                                                    ;;
                                        esac
                                        ;;
                       esac
                    done
                 fi
                 if test "x$process_now" != xno; then :
  case $lib_enablement in #(
  check) :
    failure="" ;; #(
  needed) :
    failure="Library $lib is unavailable and a prerequisite of a selected library" ;; #(
  yes) :
    failure="Required library $lib is unavailable" ;; #(
  *) :
    trace 1 "Assert - Should never fall through here"
                                ;;
esac
                        eval test_lang="\$with_${lib}_lang"
                        test_lang=`echo $test_lang`
                        # Set test_lang from $with_$Lib_lang and strip external spaces

                                                                                                case "$test_lang" in #(
  "C") :
    ac_ext=c
ac_cpp='$CPP $CPPFLAGS'
