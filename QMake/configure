as_fn_error $? "Asking to use specific header files for CXSparse via --with-CXSparse-include but --with-CXSparse-pkgconf has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                       else
                                          if ( test X$with_CXSparse_library = Xno )
                                          then
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for CXSparse via --with-CXSparse-include but --with-CXSparse-library has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          else
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for CXSparse via --with-CXSparse-include but --with-CXSparse has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          fi
                                       fi
                                    else
                                       if ( test X$use_CXSparse = Xcheck )
                                       then
                                          # Can never be here because of
                                          # --with-<lib>-pkgconf
                                          if ( test X$with_CXSparse_library = Xcheck )
                                          then
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to use specific header files for CXSparse via --with-CXSparse-include despite --with-CXSparse-library having asked to check for the package" >&5
$as_echo "$as_me: WARNING: Asking to use specific header files for CXSparse via --with-CXSparse-include despite --with-CXSparse-library having asked to check for the package" >&2;}
                                          else
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to use specific header files for CXSparse via --with-CXSparse-include despite --with-CXSparse having asked to check for the package" >&5
$as_echo "$as_me: WARNING: Asking to use specific header files for CXSparse via --with-CXSparse-include despite --with-CXSparse having asked to check for the package" >&2;}
                                          fi
                                       fi
                                    fi
                                 fi
                                 use_CXSparse=yes
                                 ;;
                       esac

else
  trace 4 No --with-CXSparse-include specified
                       with_CXSparse_include=notset


fi


          if ( test X$use_CXSparse != Xno )
          then
             # Package is not disabled, need to determine what to test with

             if ( test "X$with_CXSparse_check" != "Xpkgconfig" )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-CXSparse-library is a specification rather than a
                # simple switch
                if ( test "$with_CXSparse_library" != notset && test "$with_CXSparse_library" != no && test "$with_CXSparse_library" != check && test "$with_CXSparse_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_CXSparse_lnklibs and include
                   # the initial part of the link line in with_CXSparse_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_CXSparse_lnklibs=""
                   with_CXSparse_libpath=""
                   use_CXSparse=yes
                   trace 6 Parsing --with-CXSparse-library setting
                   with_CXSparse_library=`echo " $with_CXSparse_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_CXSparse_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_CXSparse_libpath="$with_CXSparse_libpath$with_CXSparse_lnklibs$part "
                              with_CXSparse_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_CXSparse_lnklibs="$with_CXSparse_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_CXSparse_libpath="$with_CXSparse_libpath$with_CXSparse_lnklibs$part "
                              with_CXSparse_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_CXSparse_libpath="$with_CXSparse_libpath${with_CXSparse_lnklibs}-L$part "
                                 with_CXSparse_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_CXSparse_libpath="$with_CXSparse_libpath$with_CXSparse_lnklibs-L$path "
                                          with_CXSparse_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_CXSparse_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-CXSparse-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-CXSparse-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-CXSparse-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-CXSparse-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_CXSparse_lnklibs="${with_CXSparse_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-CXSparse-library is [$with_CXSparse_libpath $with_CXSparse_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_CXSparse_libpath $with_CXSparse_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_CXSparse specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_CXSparse_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_CXSparse_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_CXSparse_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-CXSparse-library, using --with-CXSparse value [$with_CXSparse_lnklibs]
                      else
                         trace 3 No library name specified by --with-CXSparse-library, using default ["-lcxsparse"]
                         with_CXSparse_lnklibs="-lcxsparse"
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_CXSparse specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_CXSparse_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_CXSparse_libpath="-L$fallback_path $with_CXSparse_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-CXSparse-library, using --with-CXSparse to set library path for CXSparse [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_CXSparse_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-CXSparse-library and --with-CXSparse specify different paths, --with-CXSparse path ignored" >&5
$as_echo "$as_me: WARNING: --with-CXSparse-library and --with-CXSparse specify different paths, --with-CXSparse path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_CXSparse setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_CXSparse_libpath $with_CXSparse_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-CXSparse-library and --with-CXSparse specify different libraries, --with-CXSparse library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-CXSparse-library and --with-CXSparse specify different libraries, --with-CXSparse library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_CXSparse specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_CXSparse_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-CXSparse-library, using --with-CXSparse value [-L$fallback_path]
                            with_CXSparse_libpath="-L$fallback_path $with_CXSparse_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_CXSparse_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-CXSparse-library and --with-CXSparse specify different paths, --with-CXSparse path ignored" >&5
$as_echo "$as_me: WARNING: --with-CXSparse-library and --with-CXSparse specify different paths, --with-CXSparse path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-CXSparse-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-CXSparse to set library path for CXSparse [-L$fallback_path]
                      with_CXSparse_libpath=-L$fallback_path
                      use_CXSparse=yes
                   else
                      with_CXSparse_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_CXSparse specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_CXSparse_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_CXSparse_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-CXSparse pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-CXSparse pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_CXSparse_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-CXSparse to set library name for CXSparse [$with_CXSparse_lnklibs]
                   else
                      trace 3 Using default library name for CXSparse ["-lcxsparse"]
                      with_CXSparse_lnklibs="-lcxsparse"
                   fi
                fi


                # Check if --with-CXSparse-include is a path parameter
                if ( test "$with_CXSparse_include" != notset && test "$with_CXSparse_include" != no && test "$with_CXSparse_include" != check && test "$with_CXSparse_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_CXSparse_inc=""
                   use_CXSparse=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-CXSparse-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_CXSparse_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for CXSparse
                         with_CXSparse_inc="$with_CXSparse_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-CXSparse-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-CXSparse-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_CXSparse_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-CXSparse value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-CXSparse value [-I$fallback_path]" >&2;}
                         with_CXSparse_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_CXSparse_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-CXSparse-include and --with-CXSparse specify different paths, --with-CXSparse ignored" >&5
