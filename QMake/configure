$as_echo "$as_me: WARNING: Asking to check for UMFPack via --with-UMFPack but --with-UMFPack-library" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-UMFPack ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-UMFPack ignored." >&2;}
                      use_UMFPack=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for UMFPack via --with-UMFPack-library but --with-UMFPack" >&5
$as_echo "$as_me: WARNING: Explicitly asking for UMFPack via --with-UMFPack-library but --with-UMFPack" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-UMFPack ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-UMFPack ignored." >&2;}
                      use_UMFPack=yes
                   fi
                fi

                if ( test "$with_UMFPack_include" != check && test "$with_UMFPack_include" != notset )
                then
                   if ( test "$with_UMFPack_include" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for UMFPack via --with-UMFPack but --with-UMFPack-include" >&5
$as_echo "$as_me: WARNING: Asking to check for UMFPack via --with-UMFPack but --with-UMFPack-include" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-UMFPack ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-UMFPack ignored." >&2;}
                      use_UMFPack=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for UMFPack via --with-UMFPack-include but --with-UMFPack" >&5
$as_echo "$as_me: WARNING: Explicitly asking for UMFPack via --with-UMFPack-include but --with-UMFPack" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-UMFPack ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-UMFPack ignored." >&2;}
                      use_UMFPack=yes
                   fi
                fi

             else
                # --with-UMFPack != no && != check
                if ( test $with_UMFPack != notset )
                then
                   # --with-UMFPack = path or yes

                   # We need not check --with-UMFPack-pkgconf as there is no
                   # possible conflict (cannot be check and we take no to
                   # be a request to disable pkg-config, rather than the
                   # library as a whole)

                   if ( test "$with_UMFPack_library" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-UMFPack-library disabling UMFPack but --with-UMFPack enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_UMFPack_library" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling UMFPack via --with-UMFPack but --with-UMFPack-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling UMFPack via --with-UMFPack but --with-UMFPack-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-UMFPack-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-UMFPack-include ignored." >&2;}
                         use_UMFPack=no
                      fi
                   fi

                   if ( test "$with_UMFPack_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-UMFPack-include disabling UMFPack but --with-UMFPack enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_UMFPack_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling UMFPack via --with-UMFPack but --with-UMFPack-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling UMFPack via --with-UMFPack but --with-UMFPack-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-UMFPack-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-UMFPack-include ignored." >&2;}
                         use_UMFPack=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-UMFPack-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_UMFPack_pkgconf != notset && test $with_UMFPack_pkgconf != no )
