                                    fi
                                    use_vtkFiltering=no
                                 fi
                                 with_vtkFiltering=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-vtkFiltering enabling vtkFiltering libraries
                                 if ( test X$use_vtkFiltering = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable vtkFiltering by --with-vtkFiltering but --with-vtkFiltering-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_vtkFiltering = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling vtkFiltering via --with-vtkFiltering despite --with-vtkFiltering-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling vtkFiltering via --with-vtkFiltering despite --with-vtkFiltering-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_vtkFiltering=yes
                                 fi
                                 with_vtkFiltering=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-vtkFiltering checking for vtkFiltering libraries
                                 if (test X$use_vtkFiltering = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering asking to check for the libs but --with-vtkFiltering-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering asking to check for the libs but --with-vtkFiltering-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_vtkFiltering = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering asking to check for the libs but --with-vtkFiltering-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering asking to check for the libs but --with-vtkFiltering-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_vtkFiltering=check
                                    fi
                                 fi
                                 with_vtkFiltering=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_vtkFiltering )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_vtkFiltering
                                    fallback_file=""
                                    trace 3 with-vtkFiltering provides a fallback path [$fallback_path]
                                    use_vtkFiltering=yes
                                    with_vtkFiltering=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_vtkFiltering ||
$as_expr X$with_vtkFiltering : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_vtkFiltering : 'X\(//\)[^/]' \| \
	 X$with_vtkFiltering : 'X\(//\)$' \| \
	 X$with_vtkFiltering : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_vtkFiltering |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_vtkFiltering" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_vtkFiltering`
                                       if ( test X$with_vtkFiltering = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-vtkFiltering provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_vtkFiltering=yes
                                          with_vtkFiltering=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-vtkFiltering, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-vtkFiltering, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-vtkFiltering=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-vtkFiltering, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-vtkFiltering, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-vtkFiltering specified

fi






