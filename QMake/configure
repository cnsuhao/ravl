                                 fi
                                 with_GLUT=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-GLUT enabling GLUT libraries
                                 if ( test X$use_GLUT = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable GLUT by --with-GLUT but --with-GLUT-pkgconf has specifically disabled it
