                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for vtkFiltering via --with-vtkFiltering but --with-vtkFiltering-library" >&5
$as_echo "$as_me: WARNING: Asking to check for vtkFiltering via --with-vtkFiltering but --with-vtkFiltering-library" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-vtkFiltering ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-vtkFiltering ignored." >&2;}
