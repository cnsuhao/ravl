                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-avifile, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-avifile, parameter ignored" >&2;}
