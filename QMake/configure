                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Math-include asking to check for Math but --with-Math-library has" >&5
$as_echo "$as_me: WARNING: --with-Math-include asking to check for Math but --with-Math-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-Math-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-Math-include ignored." >&2;}
