$as_echo X$with_Loudmouth |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_Loudmouth" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_Loudmouth`
                                       if ( test X$with_Loudmouth = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-Loudmouth provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_Loudmouth=yes
                                          with_Loudmouth=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Loudmouth, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Loudmouth, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-Loudmouth=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Loudmouth, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Loudmouth, parameter ignored" >&2;}
