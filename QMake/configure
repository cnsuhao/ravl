                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling GLUT via --with-GLUT-library despite --with-GLUT having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling GLUT via --with-GLUT-library despite --with-GLUT having asked to check for it" >&2;}
