                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling QtGui via --with-QtGui despite --with-QtGui-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling QtGui via --with-QtGui despite --with-QtGui-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_QtGui=yes
                                 fi
                                 with_QtGui=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-QtGui checking for QtGui libraries
                                 if (test X$use_QtGui = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-QtGui asking to check for the libs but --with-QtGui-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-QtGui asking to check for the libs but --with-QtGui-pkgconf has specifically enabled it" >&2;}
                                 else
