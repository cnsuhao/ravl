                                 else
                                    if ( test X$use_Xmu = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling Xmu via --with-Xmu despite --with-Xmu-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling Xmu via --with-Xmu despite --with-Xmu-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Xmu=yes
                                 fi
                                 with_Xmu=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-Xmu checking for Xmu libraries
                                 if (test X$use_Xmu = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Xmu asking to check for the libs but --with-Xmu-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-Xmu asking to check for the libs but --with-Xmu-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_Xmu = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Xmu asking to check for the libs but --with-Xmu-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-Xmu asking to check for the libs but --with-Xmu-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_Xmu=check
                                    fi
                                 fi
                                 with_Xmu=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
