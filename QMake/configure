if test "${with_GLUT+set}" = set; then :
  withval=$with_GLUT; case $with_GLUT in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-GLUT disabling GLUT libraries
                                 if ( test X$use_GLUT = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
