                         fi
                         trace 3 Using file component of --with-Socket to set library name for Socket [$with_Socket_lnklibs]
                   else
                      trace 3 Using default library name for Socket [" | -lsocket | -lsocket -lnsl "]
                      with_Socket_lnklibs=" | -lsocket | -lsocket -lnsl "
                   fi
                fi


                # Check if --with-Socket-include is a path parameter
                if ( test "$with_Socket_include" != notset && test "$with_Socket_include" != no && test "$with_Socket_include" != check && test "$with_Socket_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_Socket_inc=""
                   use_Socket=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-Socket-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_Socket_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for Socket
                         with_Socket_inc="$with_Socket_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-Socket-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-Socket-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_Socket_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Socket value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-Socket value [-I$fallback_path]" >&2;}
                         with_Socket_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_Socket_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-include and --with-Socket specify different paths, --with-Socket ignored" >&5
$as_echo "$as_me: WARNING: --with-Socket-include and --with-Socket specify different paths, --with-Socket ignored" >&2;}
