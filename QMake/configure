                      with_libmkl_inc=-I$fallback_path
                      use_libmkl=yes
                   else
                      trace 3 Will use default system include path
                      with_libmkl_inc=""
                   fi
                fi
             fi # End of: if ( test $with_libmkl_check !- pkgconfig
          else
             trace 4 Library libmkl disabled
          fi



          with_libmkl_tstcd='DFTI_DESCRIPTOR_HANDLE * pHandle;
