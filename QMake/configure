    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_Xmu" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_Xmu`
                                       if ( test X$with_Xmu = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-Xmu provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_Xmu=yes
                                          with_Xmu=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Xmu, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Xmu, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-Xmu=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Xmu, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Xmu, parameter ignored" >&2;}
