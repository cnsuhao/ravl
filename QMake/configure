                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-Xmu disabling Xmu libraries
                                 if ( test X$use_Xmu = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable Xmu by --with-Xmu but --with-Xmu-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Xmu = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling Xmu via --with-Xmu despite --with-Xmu-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling Xmu via --with-Xmu despite --with-Xmu-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Xmu=no
                                 fi
                                 with_Xmu=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-Xmu enabling Xmu libraries
                                 if ( test X$use_Xmu = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable Xmu by --with-Xmu but --with-Xmu-pkgconf has specifically disabled it
