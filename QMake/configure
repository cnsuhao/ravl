                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Socket value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-Socket value [-I$fallback_path]" >&2;}
                         with_Socket_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_Socket_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-include and --with-Socket specify different paths, --with-Socket ignored" >&5
$as_echo "$as_me: WARNING: --with-Socket-include and --with-Socket specify different paths, --with-Socket ignored" >&2;}
                      fi
                   fi
                else
                   # --with-Socket-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-Socket-include, using --with-Socket value [-I$fallback_path]
                      with_Socket_inc=-I$fallback_path
                      use_Socket=yes
                   else
                      trace 3 Will use default system include path
                      with_Socket_inc=""
                   fi
                fi
             fi # End of: if ( test $with_Socket_pkgconf = no || notset
          else
             trace 4 Library Socket disabled
          fi

          KnownLibs=$KnownLibs" "Socket
          if ( test X$use_Socket = X )
          then
             use_Socket=check
             UnconfiguredLibs=$UnconfiguredLibs" "Socket
          else
             ConfiguredLibs=$ConfiguredLibs" "Socket
          fi

          with_Socket_tstcd='int ret, domain, type, protocol;
                               ret=socket (domain, type, protocol);
                              '
                                                  with_Socket_prq=""
          with_Socket_hdr="sys/socket.h"
          with_Socket_lang="C
                             "
          with_Socket_virtual="no"






