$as_echo "$as_me: WARNING: Asking to check for JPEG via --with-JPEG-include but --with-JPEG-pkgconf has specifically disabled it" >&2;}
                                       else
                                          if ( test X$with_JPEG_library = Xno )
                                          then
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for JPEG via --with-JPEG-include but --with-JPEG-library has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for JPEG via --with-JPEG-include but --with-JPEG-library has specifically disabled it" >&2;}
                                          else
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for JPEG via --with-JPEG-include but --with-JPEG has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for JPEG via --with-JPEG-include but --with-JPEG has specifically disabled it" >&2;}
                                          fi
                                       fi
                                    else
                                       if ( test X$use_JPEG = Xcheck )
                                       then
