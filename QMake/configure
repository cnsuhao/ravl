# Check whether --with-CXSparse was given.
if test "${with_CXSparse+set}" = set; then :
  withval=$with_CXSparse; case $with_CXSparse in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-CXSparse disabling CXSparse libraries
                                 if ( test X$use_CXSparse = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable CXSparse by --with-CXSparse but --with-CXSparse-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_CXSparse = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling CXSparse via --with-CXSparse despite --with-CXSparse-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling CXSparse via --with-CXSparse despite --with-CXSparse-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_CXSparse=no
                                 fi
                                 with_CXSparse=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-CXSparse enabling CXSparse libraries
                                 if ( test X$use_CXSparse = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable CXSparse by --with-CXSparse but --with-CXSparse-pkgconf has specifically disabled it
