                      with_LibJasper_inc=-I$fallback_path
                      use_LibJasper=yes
                   else
                      trace 3 Will use default system include path
                      with_LibJasper_inc=""
                   fi
                fi
             fi # End of: if ( test $with_LibJasper_check !- pkgconfig
          else
             trace 4 Library LibJasper disabled
          fi



