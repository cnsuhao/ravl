    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_avifile" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_avifile`
                                       if ( test X$with_avifile = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-avifile provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_avifile=yes
                                          with_avifile=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-avifile, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-avifile, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
