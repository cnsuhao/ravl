                            trace 3 No path specified by --with-vtkFiltering-library, using --with-vtkFiltering value [-L$fallback_path]
                            with_vtkFiltering_libpath="-L$fallback_path $with_vtkFiltering_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_vtkFiltering_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-library and --with-vtkFiltering specify different paths, --with-vtkFiltering path ignored" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-library and --with-vtkFiltering specify different paths, --with-vtkFiltering path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-vtkFiltering-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
