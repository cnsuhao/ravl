# Check whether --with-UMFPack-include was given.
if test "${with_UMFPack_include+set}" = set; then :
  withval=$with_UMFPack_include; case "$with_UMFPack_include" in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
