# Check whether --with-LibTIFF was given.
if test "${with_LibTIFF+set}" = set; then :
  withval=$with_LibTIFF; case $with_LibTIFF in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-LibTIFF disabling LibTIFF libraries
                                 if ( test X$use_LibTIFF = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable LibTIFF by --with-LibTIFF but --with-LibTIFF-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_LibTIFF = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling LibTIFF via --with-LibTIFF despite --with-LibTIFF-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling LibTIFF via --with-LibTIFF despite --with-LibTIFF-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_LibTIFF=no
                                 fi
                                 with_LibTIFF=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-LibTIFF enabling LibTIFF libraries
                                 if ( test X$use_LibTIFF = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable LibTIFF by --with-LibTIFF but --with-LibTIFF-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_LibTIFF = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling LibTIFF via --with-LibTIFF despite --with-LibTIFF-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling LibTIFF via --with-LibTIFF despite --with-LibTIFF-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_LibTIFF=yes
                                 fi
                                 with_LibTIFF=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-LibTIFF checking for LibTIFF libraries
                                 if (test X$use_LibTIFF = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF asking to check for the libs but --with-LibTIFF-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF asking to check for the libs but --with-LibTIFF-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_LibTIFF = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-LibTIFF asking to check for the libs but --with-LibTIFF-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-LibTIFF asking to check for the libs but --with-LibTIFF-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_LibTIFF=check
                                    fi
                                 fi
                                 with_LibTIFF=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_LibTIFF )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_LibTIFF
                                    fallback_file=""
                                    trace 3 with-LibTIFF provides a fallback path [$fallback_path]
                                    use_LibTIFF=yes
                                    with_LibTIFF=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_LibTIFF ||
$as_expr X$with_LibTIFF : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_LibTIFF : 'X\(//\)[^/]' \| \
	 X$with_LibTIFF : 'X\(//\)$' \| \
	 X$with_LibTIFF : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_LibTIFF |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_LibTIFF" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_LibTIFF`
                                       if ( test X$with_LibTIFF = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-LibTIFF provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_LibTIFF=yes
                                          with_LibTIFF=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-LibTIFF, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-LibTIFF, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-LibTIFF=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-LibTIFF, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-LibTIFF, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-LibTIFF specified

fi






