# Check whether --with-vlfeat-include was given.
if test "${with_vlfeat_include+set}" = set; then :
  withval=$with_vlfeat_include; case "$with_vlfeat_include" in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                          use_vlfeat=no
                                          with_vlfeat_include=no
                                          ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                          use_vlfeat=yes
                                          with_vlfeat_include=yes
                                          ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                          use_vlfeat=check
                                          with_vlfeat_include=check
                                          ;;
                        * )               use_vlfeat=yes
                                          ;;
                       esac

else
  trace 4 No --with-vlfeat-include specified
                       with_vlfeat_include=notset


fi

          # Having read the command-line options, check none are mutually
          # exclusive or contradictory
          #
          # First test --with-vlfeat compatability with the rest
          if ( test $with_vlfeat = no )
          then
             if ( test $with_vlfeat_pkgconf != no && test $with_vlfeat_pkgconf != notset )
             then
                # --with-vlfeat-pkgconf=path (is neither no or notset and cannot
                # be yes or check as they would have errored above.
                { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "Asking to vlfeat via pkg-config but --with-vlfeat disabled it
See \`config.log' for more details." "$LINENO" 5; }
             fi

             if ( test "$with_vlfeat_library" != no && test "$with_vlfeat_library" != notset )
             then
                if ( test "$with_vlfeat_library" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat specifically disables library but --with-vlfeat-library" >&5
$as_echo "$as_me: WARNING: --with-vlfeat specifically disables library but --with-vlfeat-library" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-vlfeat-library ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-vlfeat-library ignored" >&2;}
                   use_vlfeat=no
                else
                   # --with-vlfeat-library is yes or points to the library
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-library asking to use vlfeat but --with-vlfeat disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
              fi

             if ( test "$with_vlfeat_include" != no && test "$with_vlfeat_include" != notset )
             then
                if ( test "$with_vlfeat_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat specifically disables library but --with-vlfeat-include" >&5
$as_echo "$as_me: WARNING: --with-vlfeat specifically disables library but --with-vlfeat-include" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-vlfeat-include ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-vlfeat-include ignored" >&2;}
                   use_vlfeat=no
                else
                   # --with-vlfeat-include is yes or points to the include
                   # directory
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-include asking to use vlfeat but --with-vlfeat disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

          else
             # test --with-vlfeat != no

             if ( test $with_vlfeat = check )
             then
                if ( test $with_vlfeat_pkgconf != no && test $with_vlfeat_pkgconf != notset )
                then
                   # --with-vlfeat-pkgconf must point to the metafile
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for vlfeat via pkg-config but --with-vlfeat is" >&5
$as_echo "$as_me: WARNING: Explicitly asking for vlfeat via pkg-config but --with-vlfeat is" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-vlfeat ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-vlfeat ignored." >&2;}
                   use_vlfeat=yes
                fi

                if ( test "$with_vlfeat_library" != check && test "$with_vlfeat_library" != notset )
                then
                   if ( test "$with_vlfeat_library" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for vlfeat via --with-vlfeat but --with-vlfeat-library" >&5
$as_echo "$as_me: WARNING: Asking to check for vlfeat via --with-vlfeat but --with-vlfeat-library" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-vlfeat ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-vlfeat ignored." >&2;}
                      use_vlfeat=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for vlfeat via --with-vlfeat-library but --with-vlfeat" >&5
$as_echo "$as_me: WARNING: Explicitly asking for vlfeat via --with-vlfeat-library but --with-vlfeat" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-vlfeat ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-vlfeat ignored." >&2;}
                      use_vlfeat=yes
                   fi
                fi

                if ( test "$with_vlfeat_include" != check && test "$with_vlfeat_include" != notset )
                then
                   if ( test "$with_vlfeat_include" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for vlfeat via --with-vlfeat but --with-vlfeat-include" >&5
$as_echo "$as_me: WARNING: Asking to check for vlfeat via --with-vlfeat but --with-vlfeat-include" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-vlfeat ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-vlfeat ignored." >&2;}
                      use_vlfeat=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for vlfeat via --with-vlfeat-include but --with-vlfeat" >&5
$as_echo "$as_me: WARNING: Explicitly asking for vlfeat via --with-vlfeat-include but --with-vlfeat" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-vlfeat ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-vlfeat ignored." >&2;}
                      use_vlfeat=yes
                   fi
                fi

             else
                # --with-vlfeat != no && != check
                if ( test $with_vlfeat != notset )
                then
                   # --with-vlfeat = path or yes

                   # We need not check --with-vlfeat-pkgconf as there is no
                   # possible conflict (cannot be check and we take no to
                   # be a request to disable pkg-config, rather than the
                   # library as a whole)

                   if ( test "$with_vlfeat_library" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-library disabling vlfeat but --with-vlfeat enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_vlfeat_library" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling vlfeat via --with-vlfeat but --with-vlfeat-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling vlfeat via --with-vlfeat but --with-vlfeat-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-vlfeat-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-vlfeat-include ignored." >&2;}
                         use_vlfeat=no
                      fi
                   fi

                   if ( test "$with_vlfeat_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-include disabling vlfeat but --with-vlfeat enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_vlfeat_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling vlfeat via --with-vlfeat but --with-vlfeat-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling vlfeat via --with-vlfeat but --with-vlfeat-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-vlfeat-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-vlfeat-include ignored." >&2;}
                         use_vlfeat=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-vlfeat-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_vlfeat_pkgconf != notset && test $with_vlfeat_pkgconf != no )
          then
             # --with-vlfeat-pkgconf = path (cannot be yes or check)
             if ( test "$with_vlfeat_library" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-library asking to check for vlfeat but --with-vlfeat-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-library asking to check for vlfeat but --with-vlfeat-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-vlfeat-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-vlfeat-library ignored." >&2;}
                use_vlfeat=yes
             else
                if ( test "$with_vlfeat_library" != no && test "$with_vlfeat_library" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-library enabling compile check for vlfeat but --with-vlfeat-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test "$with_vlfeat_include" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-include asking to check for vlfeat but --with-vlfeat-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-include asking to check for vlfeat but --with-vlfeat-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-vlfeat-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-vlfeat-include ignored." >&2;}
                use_vlfeat=yes
             else
                if ( test "$with_vlfeat_include" != no && test "$with_vlfeat_include" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-include enabling compile check for vlfeat but --with-vlfeat-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test "$with_vlfeat_library" = no )
          then
             if ( test "$with_vlfeat_include" != no && test "$with_vlfeat_include" != notset )
             then
                if ( test "$with_vlfeat_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-include asking to check for vlfeat but --with-vlfeat-library has" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-include asking to check for vlfeat but --with-vlfeat-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-vlfeat-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-vlfeat-include ignored." >&2;}
                   use_vlfeat=no
                else
                   # --with-vlfeat-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-include enabling vlfeat but --with-vlfeat-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-vlfeat-library != no
             if ( test "$with_vlfeat_library" = check )
             then
                if ( test "$with_vlfeat_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-library asking to check for vlfeat but --with-vlfeat-include has" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-library asking to check for vlfeat but --with-vlfeat-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-vlfeat-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-vlfeat-library ignored." >&2;}
                   use_vlfeat=no
                else
                   if ( test "$with_vlfeat_include" != check && test "$with_vlfeat_include" != notset )
                   then
                      # --with-vlfeat-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-library asking to check for vlfeat but --with-vlfeat-include has" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-library asking to check for vlfeat but --with-vlfeat-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-vlfeat-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-vlfeat-library ignored." >&2;}
                      use_vlfeat=yes
                   fi
                fi
             else
                if ( test "$with_vlfeat_library" != notset )
                then
                   # --with-vlfeat-library a path or yes
                   if ( test "$with_vlfeat_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-vlfeat-library enabling vlfeat but --with-vlfeat-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_vlfeat_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-include asking to check for vlfeat but --with-vlfeat-library has" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-include asking to check for vlfeat but --with-vlfeat-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-vlfeat-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-vlfeat-include ignored." >&2;}
                         use_vlfeat=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-vlfeat to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_vlfeat != notset && test $with_vlfeat != no && test $with_vlfeat != check && test $with_vlfeat != yes )
          then
             # We have a path to use
             if ( test -d $with_vlfeat )
             then
                # Parameter is a directory path
                fallback_path=$with_vlfeat
                trace 3 with-vlfeat provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_vlfeat ||
$as_expr X$with_vlfeat : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_vlfeat : 'X\(//\)[^/]' \| \
	 X$with_vlfeat : 'X\(//\)$' \| \
	 X$with_vlfeat : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_vlfeat |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_vlfeat" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_vlfeat`
                   if ( test X$with_vlfeat = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-vlfeat provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-vlfeat, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-vlfeat, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-vlfeat=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-vlfeat, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-vlfeat, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_vlfeat != Xno )
          then
             if ( test $with_vlfeat_pkgconf = no ||  test $with_vlfeat_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-vlfeat-library is a specification rather than a
                # simple switch
                if ( test "$with_vlfeat_library" != notset && test "$with_vlfeat_library" != no && test "$with_vlfeat_library" != check && test "$with_vlfeat_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_vlfeat_lnklibs and include
                   # the initial part of the link line in with_vlfeat_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_vlfeat_lnklibs=""
                   with_vlfeat_libpath=""
                   use_vlfeat=yes
                   trace 6 Parsing --with-vlfeat-library setting
                   with_vlfeat_library=`echo " $with_vlfeat_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_vlfeat_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_vlfeat_libpath="$with_vlfeat_libpath$with_vlfeat_lnklibs$part "
                              with_vlfeat_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_vlfeat_lnklibs="$with_vlfeat_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_vlfeat_libpath="$with_vlfeat_libpath$with_vlfeat_lnklibs$part "
                              with_vlfeat_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_vlfeat_libpath="$with_vlfeat_libpath${with_vlfeat_lnklibs}-L$part "
                                 with_vlfeat_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vlfeat_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vlfeat_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vlfeat_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-vlfeat_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_vlfeat_libpath="$with_vlfeat_libpath$with_vlfeat_lnklibs-L$path "
                                          with_vlfeat_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_vlfeat_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-vlfeat-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-vlfeat-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-vlfeat-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-vlfeat-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vlfeat_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vlfeat_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vlfeat_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-vlfeat_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_vlfeat_lnklibs="${with_vlfeat_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-vlfeat-library is [$with_vlfeat_libpath $with_vlfeat_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_vlfeat_libpath $with_vlfeat_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_vlfeat specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_vlfeat_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vlfeat pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-vlfeat pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_vlfeat_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-vlfeat pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-vlfeat pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_vlfeat_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-vlfeat-library, using --with-vlfeat value [$with_vlfeat_lnklibs]
                      else
                         trace 3 No library name specified by --with-vlfeat-library, using default ["-lvl"]
                         with_vlfeat_lnklibs="-lvl"
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_vlfeat specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_vlfeat_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_vlfeat_libpath="-L$fallback_path $with_vlfeat_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-vlfeat-library, using --with-vlfeat to set library path for vlfeat [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_vlfeat_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-library and --with-vlfeat specify different paths, --with-vlfeat path ignored" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-library and --with-vlfeat specify different paths, --with-vlfeat path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_vlfeat setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_vlfeat_libpath $with_vlfeat_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vlfeat-library and --with-vlfeat specify different libraries, --with-vlfeat library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-vlfeat-library and --with-vlfeat specify different libraries, --with-vlfeat library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_vlfeat specified a fallback path, are we using
