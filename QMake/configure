                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Math-library and --with-Math specify different paths, --with-Math path ignored" >&5
$as_echo "$as_me: WARNING: --with-Math-library and --with-Math specify different paths, --with-Math path ignored" >&2;}
                            fi
