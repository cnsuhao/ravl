                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-avifile disabling avifile libraries
                                 if ( test X$use_avifile = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable avifile by --with-avifile but --with-avifile-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_avifile = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling avifile via --with-avifile despite --with-avifile-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling avifile via --with-avifile despite --with-avifile-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_avifile=no
                                 fi
                                 with_avifile=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-avifile enabling avifile libraries
                                 if ( test X$use_avifile = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
