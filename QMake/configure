          # Having read the command-line options, check none are mutually
          # exclusive or contradictory
          #
          # First test --with-vtkFiltering compatability with the rest
          if ( test $with_vtkFiltering = no )
          then
             if ( test $with_vtkFiltering_pkgconf != no && test $with_vtkFiltering_pkgconf != notset )
             then
                # --with-vtkFiltering-pkgconf=path (is neither no or notset and cannot
