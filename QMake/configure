                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                          use_UMFPack=no
                                          with_UMFPack_library=no
                                          ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                          use_UMFPack=yes
                                          with_UMFPack_library=yes
                                          ;;
