                                 trace 4 --with-UMFPack-include disabling UMFPack libraries
                                 if ( test X$use_UMFPack = Xyes )
                                 then
                                    if ( test X$with_UMFPack_check = Xpkgconfig )
                                    then
                                       # Either --with-<lib>-pkgconf is in use
                                       # or --with-<lib> specifies a .pc file
                                       { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable UMFPack via --with-UMFPack-include but another configuration option has specifically asked to use pkg-config
See \`config.log' for more details" "$LINENO" 5; }
                                    else
                                       # As use_<lib>=yes, the only possible
                                       # values for --with-<lib>-library are:
                                       # notset; yes; check or a path. So, we
                                       # can check for the conflicting setting
                                       # of yes or any path by determining the
                                       # setting is not check or notset.
                                       if ( test X$with_UMFPack_library != Xnotset && test X$with_UMFPack_library != Xcheck )
                                       then
                                          { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable UMFPack via --with-UMFPack-include but --with-UMFPack-library has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
