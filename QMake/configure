                                 if (test X$use_tetgen = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen asking to check for the libs but --with-tetgen-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-tetgen asking to check for the libs but --with-tetgen-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_tetgen = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-tetgen asking to check for the libs but --with-tetgen-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-tetgen asking to check for the libs but --with-tetgen-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_tetgen=check
                                    fi
                                 fi
                                 with_tetgen=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_tetgen )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_tetgen
                                    fallback_file=""
                                    trace 3 with-tetgen provides a fallback path [$fallback_path]
                                    use_tetgen=yes
                                    with_tetgen=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_tetgen ||
$as_expr X$with_tetgen : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_tetgen : 'X\(//\)[^/]' \| \
	 X$with_tetgen : 'X\(//\)$' \| \
	 X$with_tetgen : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_tetgen |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_tetgen" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_tetgen`
                                       if ( test X$with_tetgen = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-tetgen provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_tetgen=yes
                                          with_tetgen=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-tetgen, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-tetgen, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-tetgen=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-tetgen, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-tetgen, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-tetgen specified

fi






