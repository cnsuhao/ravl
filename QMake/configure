# Check whether --with-DynLink-include was given.
if test "${with_DynLink_include+set}" = set; then :
  withval=$with_DynLink_include; case "$with_DynLink_include" in
                          'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-DynLink-include disabling DynLink libraries
                                 if ( test X$use_DynLink = Xyes )
                                 then
                                    if ( test X$with_DynLink_check = Xpkgconfig )
                                    then
                                       # Either --with-<lib>-pkgconf is in use
                                       # or --with-<lib> specifies a .pc file
                                       { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable DynLink via --with-DynLink-include but another configuration option has specifically asked to use pkg-config
See \`config.log' for more details" "$LINENO" 5; }
                                    else
                                       # As use_<lib>=yes, the only possible
                                       # values for --with-<lib>-library are:
                                       # notset; yes; check or a path. So, we
                                       # can check for the conflicting setting
                                       # of yes or any path by determining the
                                       # setting is not check or notset.
