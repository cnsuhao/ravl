                                    fi
                                    use_LibJasper=no
                                 fi
                                 with_LibJasper=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-LibJasper enabling LibJasper libraries
                                 if ( test X$use_LibJasper = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable LibJasper by --with-LibJasper but --with-LibJasper-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
