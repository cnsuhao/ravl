                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Qt3QGLViewer_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-Qt3QGLViewer_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
