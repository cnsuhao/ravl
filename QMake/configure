                               with_libmkl_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-libmkl pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-libmkl pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_libmkl_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-libmkl to set library name for libmkl [$with_libmkl_lnklibs]
                   else
                      trace 3 Using default library name for libmkl ["-lmkl_rt"]
                      with_libmkl_lnklibs="-lmkl_rt"
                   fi
                fi


                # Check if --with-libmkl-include is a path parameter
                if ( test "$with_libmkl_include" != notset && test "$with_libmkl_include" != no && test "$with_libmkl_include" != check && test "$with_libmkl_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_libmkl_inc=""
                   use_libmkl=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-libmkl-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_libmkl_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for libmkl
                         with_libmkl_inc="$with_libmkl_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
