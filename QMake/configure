$as_echo "$as_me: WARNING: --with-vtkCommon asking to check for the libs but --with-vtkCommon-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_vtkCommon = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkCommon asking to check for the libs but --with-vtkCommon-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-vtkCommon asking to check for the libs but --with-vtkCommon-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_vtkCommon=check
                                    fi
                                 fi
                                 with_vtkCommon=check
                                 ;;
