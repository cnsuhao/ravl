# Check whether --with-vtkFiltering-include was given.
if test "${with_vtkFiltering_include+set}" = set; then :
  withval=$with_vtkFiltering_include; case "$with_vtkFiltering_include" in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
