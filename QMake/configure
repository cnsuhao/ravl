                                 if (test X$use_GLUT = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT asking to check for the libs but --with-GLUT-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-GLUT asking to check for the libs but --with-GLUT-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_GLUT = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-GLUT asking to check for the libs but --with-GLUT-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-GLUT asking to check for the libs but --with-GLUT-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_GLUT=check
                                    fi
                                 fi
                                 with_GLUT=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_GLUT )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_GLUT
                                    fallback_file=""
                                    trace 3 with-GLUT provides a fallback path [$fallback_path]
                                    use_GLUT=yes
                                    with_GLUT=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_GLUT ||
$as_expr X$with_GLUT : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_GLUT : 'X\(//\)[^/]' \| \
	 X$with_GLUT : 'X\(//\)$' \| \
	 X$with_GLUT : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_GLUT |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_GLUT" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_GLUT`
                                       if ( test X$with_GLUT = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-GLUT provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_GLUT=yes
                                          with_GLUT=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-GLUT, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-GLUT, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-GLUT=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-GLUT, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-GLUT, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-GLUT specified

fi






