                if ( test "$with_vtkFiltering_include" != notset && test "$with_vtkFiltering_include" != no && test "$with_vtkFiltering_include" != check && test "$with_vtkFiltering_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_vtkFiltering_inc=""
                   use_vtkFiltering=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-vtkFiltering-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_vtkFiltering_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for vtkFiltering
                         with_vtkFiltering_inc="$with_vtkFiltering_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-vtkFiltering-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-vtkFiltering-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_vtkFiltering_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-vtkFiltering value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-vtkFiltering value [-I$fallback_path]" >&2;}
                         with_vtkFiltering_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_vtkFiltering_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-vtkFiltering-include and --with-vtkFiltering specify different paths, --with-vtkFiltering ignored" >&5
$as_echo "$as_me: WARNING: --with-vtkFiltering-include and --with-vtkFiltering specify different paths, --with-vtkFiltering ignored" >&2;}
                      fi
                   fi
                else
                   # --with-vtkFiltering-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-vtkFiltering-include, using --with-vtkFiltering value [-I$fallback_path]
                      with_vtkFiltering_inc=-I$fallback_path
                      use_vtkFiltering=yes
                   else
                      trace 3 Will use default system include path
                      with_vtkFiltering_inc=""
                   fi
                fi
             fi # End of: if ( test $with_vtkFiltering_check !- pkgconfig
          else
             trace 4 Library vtkFiltering disabled
          fi



          with_vtkFiltering_tstcd='vtkCellArray *faces = vtkCellArray::New();'
                                                  with_vtkFiltering_prq="vtkCommon"
          with_vtkFiltering_hdr="vtkCellArray.h"
          with_vtkFiltering_lang="C++
                             "





          KnownLibs=$KnownLibs" "vtkFiltering
          if ( test X$use_vtkFiltering = X )
          then
             use_vtkFiltering=check
             UnconfiguredLibs=$UnconfiguredLibs" "vtkFiltering
          else
             ConfiguredLibs=$ConfiguredLibs" "vtkFiltering
          fi







          with_vtkGraphics_check=compile
          if ( test Xcompile = Xvirtual )
          then
             with_vtkGraphics_virtual=yes
          else
             with_vtkGraphics_virtual=no
          fi

          fallback_file=""
          fallback_path=""


