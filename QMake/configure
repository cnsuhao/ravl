                if ( test "$with_UMFPack_library" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack specifically disables library but --with-UMFPack-library" >&5
$as_echo "$as_me: WARNING: --with-UMFPack specifically disables library but --with-UMFPack-library" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-UMFPack-library ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-UMFPack-library ignored" >&2;}
                   use_UMFPack=no
                else
                   # --with-UMFPack-library is yes or points to the library
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
