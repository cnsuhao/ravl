                               # Now strip any leading -l or lib prefix
                               with_tetgen_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-tetgen pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-tetgen pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_tetgen_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-tetgen-library, using --with-tetgen value [$with_tetgen_lnklibs]
                      else
                         trace 3 No library name specified by --with-tetgen-library, using default ["-ltet"]
                         with_tetgen_lnklibs="-ltet"
                      fi

                      if ( test X$fallback_path != X )
                      then
