if test "${with_libmkl_library+set}" = set; then :
  withval=$with_libmkl_library; case "$with_libmkl_library" in
                          'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-libmkl-library disabling libmkl libraries
                                 if ( test X$use_libmkl = Xyes )
                                 then
                                    if ( test X$with_libmkl_check = Xpkgconfig )
                                    then
                                       # Either --with-<lib>-pkgconf is in use
                                       # or --with-<lib> specifies a .pc file
