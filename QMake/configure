             if ( test "$with_UMFPack_include" != no && test "$with_UMFPack_include" != notset )
             then
                if ( test "$with_UMFPack_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack specifically disables library but --with-UMFPack-include" >&5
$as_echo "$as_me: WARNING: --with-UMFPack specifically disables library but --with-UMFPack-include" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-UMFPack-include ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-UMFPack-include ignored" >&2;}
                   use_UMFPack=no
                else
                   # --with-UMFPack-include is yes or points to the include
                   # directory
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-UMFPack-include asking to use UMFPack but --with-UMFPack disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

          else
             # test --with-UMFPack != no

             if ( test $with_UMFPack = check )
             then
                if ( test $with_UMFPack_pkgconf != no && test $with_UMFPack_pkgconf != notset )
                then
                   # --with-UMFPack-pkgconf must point to the metafile
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for UMFPack via pkg-config but --with-UMFPack is" >&5
$as_echo "$as_me: WARNING: Explicitly asking for UMFPack via pkg-config but --with-UMFPack is" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-UMFPack ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-UMFPack ignored." >&2;}
                   use_UMFPack=yes
                fi

