$as_expr X$with_Yajl : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Yajl : 'X\(//\)[^/]' \| \
	 X$with_Yajl : 'X\(//\)$' \| \
	 X$with_Yajl : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Yajl |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
