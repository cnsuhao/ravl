                      with_vlfeat_inc=-I$fallback_path
                      use_vlfeat=yes
                   else
                      trace 3 Will use default system include path
                      with_vlfeat_inc=""
                   fi
                fi
             fi # End of: if ( test $with_vlfeat_pkgconf = no || notset
          else
             trace 4 Library vlfeat disabled
          fi

