# Check whether --with-LibTIFF-pkgconf was given.
if test "${with_LibTIFF_pkgconf+set}" = set; then :
  withval=$with_LibTIFF_pkgconf; case $with_LibTIFF_pkgconf in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-LibTIFF-pkgconf disabling LibTIFF
                                 with_LibTIFF_pkgconf=no
                                 use_LibTIFF=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 if ( test X != X )
                                 then
                                    trace 4 Asking for LibTIFF via --with-LibTIFF-pkgconf default value [$with_LibTIFF_pkgconf]
                                    with_LibTIFF_pkgconf=
                                    with_LibTIFF_check=pkgconfig
