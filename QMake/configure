                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack-library asking to check for UMFPack but --with-UMFPack-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-UMFPack-library asking to check for UMFPack but --with-UMFPack-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-UMFPack-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-UMFPack-library ignored." >&2;}
                use_UMFPack=yes
             else
                if ( test "$with_UMFPack_library" != no && test "$with_UMFPack_library" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-UMFPack-library enabling compile check for UMFPack but --with-UMFPack-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test "$with_UMFPack_include" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack-include asking to check for UMFPack but --with-UMFPack-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-UMFPack-include asking to check for UMFPack but --with-UMFPack-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-UMFPack-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-UMFPack-include ignored." >&2;}
                use_UMFPack=yes
             else
                if ( test "$with_UMFPack_include" != no && test "$with_UMFPack_include" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-UMFPack-include enabling compile check for UMFPack but --with-UMFPack-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test "$with_UMFPack_library" = no )
          then
             if ( test "$with_UMFPack_include" != no && test "$with_UMFPack_include" != notset )
             then
                if ( test "$with_UMFPack_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack-include asking to check for UMFPack but --with-UMFPack-library has" >&5
$as_echo "$as_me: WARNING: --with-UMFPack-include asking to check for UMFPack but --with-UMFPack-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-UMFPack-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-UMFPack-include ignored." >&2;}
                   use_UMFPack=no
                else
                   # --with-UMFPack-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-UMFPack-include enabling UMFPack but --with-UMFPack-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-UMFPack-library != no
             if ( test "$with_UMFPack_library" = check )
             then
                if ( test "$with_UMFPack_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack-library asking to check for UMFPack but --with-UMFPack-include has" >&5
$as_echo "$as_me: WARNING: --with-UMFPack-library asking to check for UMFPack but --with-UMFPack-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-UMFPack-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-UMFPack-library ignored." >&2;}
                   use_UMFPack=no
                else
                   if ( test "$with_UMFPack_include" != check && test "$with_UMFPack_include" != notset )
                   then
                      # --with-UMFPack-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-UMFPack-library asking to check for UMFPack but --with-UMFPack-include has" >&5
$as_echo "$as_me: WARNING: --with-UMFPack-library asking to check for UMFPack but --with-UMFPack-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-UMFPack-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-UMFPack-library ignored." >&2;}
