                                          # or --with-<lib>-pkgconf
                                          use_vlfeat=check
                                          # This is the only condition where
                                          # we need to be updating use_<lib>
                                       fi
                                    fi
                                 fi
                                 with_vlfeat_library=check
                                 ;;
