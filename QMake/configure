                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Qt5Sql asking to check for the libs but --with-Qt5Sql-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-Qt5Sql asking to check for the libs but --with-Qt5Sql-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_Qt5Sql=check
                                    fi
                                 fi
                                 with_Qt5Sql=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
