          # Now process --with-Math to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_Math != notset && test $with_Math != no && test $with_Math != check && test $with_Math != yes )
          then
             # We have a path to use
             if ( test -d $with_Math )
             then
                # Parameter is a directory path
                fallback_path=$with_Math
                trace 3 with-Math provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_Math ||
$as_expr X$with_Math : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Math : 'X\(//\)[^/]' \| \
	 X$with_Math : 'X\(//\)$' \| \
	 X$with_Math : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Math |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_Math" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_Math`
                   if ( test X$with_Math = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-Math provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Math, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Math, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-Math=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Math, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Math, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_Math != Xno )
          then
             if ( test $with_Math_pkgconf = no ||  test $with_Math_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-Math-library is a specification rather than a
                # simple switch
                if ( test "$with_Math_library" != notset && test "$with_Math_library" != no && test "$with_Math_library" != check && test "$with_Math_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_Math_lnklibs and include
                   # the initial part of the link line in with_Math_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_Math_lnklibs=""
                   with_Math_libpath=""
                   use_Math=yes
                   trace 6 Parsing --with-Math-library setting
                   with_Math_library=`echo " $with_Math_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_Math_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_Math_libpath="$with_Math_libpath$with_Math_lnklibs$part "
                              with_Math_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_Math_lnklibs="$with_Math_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_Math_libpath="$with_Math_libpath$with_Math_lnklibs$part "
                              with_Math_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_Math_libpath="$with_Math_libpath${with_Math_lnklibs}-L$part "
                                 with_Math_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
