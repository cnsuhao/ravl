                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_Xmu
                                    fallback_file=""
                                    trace 3 with-Xmu provides a fallback path [$fallback_path]
                                    use_Xmu=yes
                                    with_Xmu=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_Xmu ||
