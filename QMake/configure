$as_echo "$as_me: WARNING: Explicitly enabling Math via --with-Math but --with-Math-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-Math-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-Math-include ignored." >&2;}
