                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling Qt3Support via --with-Qt3Support despite --with-Qt3Support-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling Qt3Support via --with-Qt3Support despite --with-Qt3Support-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Qt3Support=no
                                 fi
                                 with_Qt3Support=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-Qt3Support enabling Qt3Support libraries
                                 if ( test X$use_Qt3Support = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
