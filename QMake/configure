$as_echo "$as_me: WARNING: Enabling FFMPEGSWS via --with-FFMPEGSWS despite --with-FFMPEGSWS-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_FFMPEGSWS=yes
                                 fi
                                 with_FFMPEGSWS=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-FFMPEGSWS checking for FFMPEGSWS libraries
                                 if (test X$use_FFMPEGSWS = Xyes )
