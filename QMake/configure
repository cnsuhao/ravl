                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                          use_UMFPack=no
                                          with_UMFPack=no
                                          ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                          use_UMFPack=yes
                                          with_UMFPack=yes
                                          ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                          use_UMFPack=check
                                          with_UMFPack=check
                                          ;;
                        * )               use_UMFPack=yes
                                          ;;
                       esac

