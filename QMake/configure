                   else
                      trace 3 Using default library name for tetgen ["-ltet"]
                      with_tetgen_lnklibs="-ltet"
                   fi
                fi


                # Check if --with-tetgen-include is a path parameter
                if ( test "$with_tetgen_include" != notset && test "$with_tetgen_include" != no && test "$with_tetgen_include" != check && test "$with_tetgen_include" != yes )
