                      fi
                   fi
                else
                   # --with-Math-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-Math to set library path for Math [-L$fallback_path]
                      with_Math_libpath=-L$fallback_path
                      use_Math=yes
                   else
                      with_Math_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
