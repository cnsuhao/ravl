                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for Socket via --with-Socket but --with-Socket-include" >&5
$as_echo "$as_me: WARNING: Asking to check for Socket via --with-Socket but --with-Socket-include" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-Socket ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-Socket ignored." >&2;}
                      use_Socket=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for Socket via --with-Socket-include but --with-Socket" >&5
$as_echo "$as_me: WARNING: Explicitly asking for Socket via --with-Socket-include but --with-Socket" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-Socket ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-Socket ignored." >&2;}
                      use_Socket=yes
                   fi
                fi

             else
                # --with-Socket != no && != check
                if ( test $with_Socket != notset )
                then
                   # --with-Socket = path or yes

                   # We need not check --with-Socket-pkgconf as there is no
                   # possible conflict (cannot be check and we take no to
                   # be a request to disable pkg-config, rather than the
                   # library as a whole)

                   if ( test "$with_Socket_library" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Socket-library disabling Socket but --with-Socket enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_Socket_library" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling Socket via --with-Socket but --with-Socket-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling Socket via --with-Socket but --with-Socket-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-Socket-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-Socket-include ignored." >&2;}
                         use_Socket=no
                      fi
                   fi

                   if ( test "$with_Socket_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Socket-include disabling Socket but --with-Socket enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_Socket_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling Socket via --with-Socket but --with-Socket-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling Socket via --with-Socket but --with-Socket-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-Socket-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-Socket-include ignored." >&2;}
                         use_Socket=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-Socket-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_Socket_pkgconf != notset && test $with_Socket_pkgconf != no )
          then
             # --with-Socket-pkgconf = path (cannot be yes or check)
             if ( test "$with_Socket_library" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-library asking to check for Socket but --with-Socket-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-Socket-library asking to check for Socket but --with-Socket-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-Socket-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-Socket-library ignored." >&2;}
                use_Socket=yes
             else
                if ( test "$with_Socket_library" != no && test "$with_Socket_library" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Socket-library enabling compile check for Socket but --with-Socket-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test "$with_Socket_include" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-include asking to check for Socket but --with-Socket-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-Socket-include asking to check for Socket but --with-Socket-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-Socket-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-Socket-include ignored." >&2;}
                use_Socket=yes
             else
                if ( test "$with_Socket_include" != no && test "$with_Socket_include" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Socket-include enabling compile check for Socket but --with-Socket-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test "$with_Socket_library" = no )
          then
             if ( test "$with_Socket_include" != no && test "$with_Socket_include" != notset )
             then
                if ( test "$with_Socket_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-include asking to check for Socket but --with-Socket-library has" >&5
$as_echo "$as_me: WARNING: --with-Socket-include asking to check for Socket but --with-Socket-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-Socket-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-Socket-include ignored." >&2;}
                   use_Socket=no
                else
                   # --with-Socket-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Socket-include enabling Socket but --with-Socket-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-Socket-library != no
             if ( test "$with_Socket_library" = check )
             then
                if ( test "$with_Socket_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-library asking to check for Socket but --with-Socket-include has" >&5
$as_echo "$as_me: WARNING: --with-Socket-library asking to check for Socket but --with-Socket-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-Socket-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-Socket-library ignored." >&2;}
                   use_Socket=no
                else
                   if ( test "$with_Socket_include" != check && test "$with_Socket_include" != notset )
                   then
                      # --with-Socket-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-library asking to check for Socket but --with-Socket-include has" >&5
$as_echo "$as_me: WARNING: --with-Socket-library asking to check for Socket but --with-Socket-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-Socket-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-Socket-library ignored." >&2;}
                      use_Socket=yes
                   fi
                fi
             else
                if ( test "$with_Socket_library" != notset )
                then
                   # --with-Socket-library a path or yes
                   if ( test "$with_Socket_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-Socket-library enabling Socket but --with-Socket-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_Socket_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-include asking to check for Socket but --with-Socket-library has" >&5
$as_echo "$as_me: WARNING: --with-Socket-include asking to check for Socket but --with-Socket-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-Socket-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-Socket-include ignored." >&2;}
                         use_Socket=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-Socket to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_Socket != notset && test $with_Socket != no && test $with_Socket != check && test $with_Socket != yes )
          then
             # We have a path to use
             if ( test -d $with_Socket )
             then
                # Parameter is a directory path
                fallback_path=$with_Socket
                trace 3 with-Socket provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_Socket ||
$as_expr X$with_Socket : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Socket : 'X\(//\)[^/]' \| \
	 X$with_Socket : 'X\(//\)$' \| \
	 X$with_Socket : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Socket |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_Socket" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_Socket`
                   if ( test X$with_Socket = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-Socket provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Socket, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Socket, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-Socket=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Socket, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Socket, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_Socket != Xno )
          then
             if ( test $with_Socket_pkgconf = no ||  test $with_Socket_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-Socket-library is a specification rather than a
                # simple switch
                if ( test "$with_Socket_library" != notset && test "$with_Socket_library" != no && test "$with_Socket_library" != check && test "$with_Socket_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_Socket_lnklibs and include
                   # the initial part of the link line in with_Socket_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_Socket_lnklibs=""
                   with_Socket_libpath=""
                   use_Socket=yes
                   trace 6 Parsing --with-Socket-library setting
                   with_Socket_library=`echo " $with_Socket_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_Socket_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_Socket_libpath="$with_Socket_libpath$with_Socket_lnklibs$part "
                              with_Socket_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_Socket_lnklibs="$with_Socket_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_Socket_libpath="$with_Socket_libpath$with_Socket_lnklibs$part "
                              with_Socket_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_Socket_libpath="$with_Socket_libpath${with_Socket_lnklibs}-L$part "
                                 with_Socket_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Socket_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Socket_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Socket_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-Socket_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_Socket_libpath="$with_Socket_libpath$with_Socket_lnklibs-L$path "
                                          with_Socket_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_Socket_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Socket-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Socket-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-Socket-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-Socket-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Socket_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Socket_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Socket_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-Socket_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_Socket_lnklibs="${with_Socket_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-Socket-library is [$with_Socket_libpath $with_Socket_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_Socket_libpath $with_Socket_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_Socket specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_Socket_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Socket pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Socket pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_Socket_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Socket pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-Socket pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_Socket_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-Socket-library, using --with-Socket value [$with_Socket_lnklibs]
                      else
