if test "${with_libmkl+set}" = set; then :
  withval=$with_libmkl; case $with_libmkl in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-libmkl disabling libmkl libraries
                                 if ( test X$use_libmkl = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable libmkl by --with-libmkl but --with-libmkl-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_libmkl = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling libmkl via --with-libmkl despite --with-libmkl-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling libmkl via --with-libmkl despite --with-libmkl-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_libmkl=no
                                 fi
                                 with_libmkl=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-libmkl enabling libmkl libraries
                                 if ( test X$use_libmkl = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable libmkl by --with-libmkl but --with-libmkl-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_libmkl = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling libmkl via --with-libmkl despite --with-libmkl-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling libmkl via --with-libmkl despite --with-libmkl-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_libmkl=yes
                                 fi
                                 with_libmkl=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-libmkl checking for libmkl libraries
                                 if (test X$use_libmkl = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-libmkl asking to check for the libs but --with-libmkl-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-libmkl asking to check for the libs but --with-libmkl-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_libmkl = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-libmkl asking to check for the libs but --with-libmkl-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-libmkl asking to check for the libs but --with-libmkl-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_libmkl=check
                                    fi
                                 fi
                                 with_libmkl=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_libmkl )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_libmkl
                                    fallback_file=""
                                    trace 3 with-libmkl provides a fallback path [$fallback_path]
                                    use_libmkl=yes
                                    with_libmkl=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_libmkl ||
$as_expr X$with_libmkl : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_libmkl : 'X\(//\)[^/]' \| \
	 X$with_libmkl : 'X\(//\)$' \| \
	 X$with_libmkl : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_libmkl |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_libmkl" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_libmkl`
                                       if ( test X$with_libmkl = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-libmkl provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_libmkl=yes
                                          with_libmkl=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-libmkl, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-libmkl, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-libmkl=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-libmkl, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-libmkl, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-libmkl specified

