else
  trace 4 No --with-Yajl-include specified
                       with_Yajl_include=notset


fi


          if ( test X$use_Yajl != Xno )
          then
             # Package is not disabled, need to determine what to test with

             if ( test "X$with_Yajl_check" != "Xpkgconfig" )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-Yajl-library is a specification rather than a
                # simple switch
                if ( test "$with_Yajl_library" != notset && test "$with_Yajl_library" != no && test "$with_Yajl_library" != check && test "$with_Yajl_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_Yajl_lnklibs and include
                   # the initial part of the link line in with_Yajl_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_Yajl_lnklibs=""
                   with_Yajl_libpath=""
                   use_Yajl=yes
                   trace 6 Parsing --with-Yajl-library setting
                   with_Yajl_library=`echo " $with_Yajl_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_Yajl_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_Yajl_libpath="$with_Yajl_libpath$with_Yajl_lnklibs$part "
                              with_Yajl_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_Yajl_lnklibs="$with_Yajl_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_Yajl_libpath="$with_Yajl_libpath$with_Yajl_lnklibs$part "
                              with_Yajl_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_Yajl_libpath="$with_Yajl_libpath${with_Yajl_lnklibs}-L$part "
                                 with_Yajl_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_Yajl_libpath="$with_Yajl_libpath$with_Yajl_lnklibs-L$path "
                                          with_Yajl_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_Yajl_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Yajl-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Yajl-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-Yajl-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Illegal value for --with-Yajl-library, [$part] ignored" >&2;}
                                    fi
                                 else
                                    # Parameter is just "something" - assume library name
                                    #
                                    # Does it have a '.a' extension
                                    lib=`$EXPR "X$part" : 'X\(.*\)\.a$'`
                                    if ( test X$lib != X  )
                                    then
                                       lib=`echo $lib | $SED 's/^lib//'`
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl_library pointing to a .a" >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                    else
                                       # Does it have a '.so' extension
                                       lib=`$EXPR "X$part" : 'X\(.*\)\.so$'`
                                       if ( test X$lib != X )
                                       then
                                          lib=`echo $lib | $SED 's/^lib//'`
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl_library pointing to" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                       else
                                          # Parameter is name fragment
                                          lib=`echo $part | $SED 's/^lib//'`
                                       fi
                                    fi
                                    TRACE_INDENT="     "
                                    trace 6 Adding [-l$lib] to library list
                                    with_Yajl_lnklibs="${with_Yajl_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-Yajl-library is [$with_Yajl_libpath $with_Yajl_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_Yajl_libpath $with_Yajl_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
                   then
                      # no -lxxx in libpath or lnklibs - use any fallback or the
                      # default library name
                      if ( test X$fallback_file != X )
                      then
                         # --with_Yajl specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_Yajl_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_Yajl_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_Yajl_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 No library name specified by --with-Yajl-library, using --with-Yajl value [$with_Yajl_lnklibs]
                      else
                         trace 3 No library name specified by --with-Yajl-library, using default ["-lyajl"]
                         with_Yajl_lnklibs="-lyajl"
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_Yajl specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_Yajl_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_Yajl_libpath="-L$fallback_path $with_Yajl_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-Yajl-library, using --with-Yajl to set library path for Yajl [-L$fallback_path]
                         else
                            if ( test X`$EXPR "X $with_Yajl_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Yajl-library and --with-Yajl specify different paths, --with-Yajl path ignored" >&5
$as_echo "$as_me: WARNING: --with-Yajl-library and --with-Yajl specify different paths, --with-Yajl path ignored" >&2;}
                            fi
                         fi
                      fi
                   else
                      # We already have a library specified, confirm that any
                      # --with_Yajl setting agreed
                      if ( test X$fallback_file != X )
                      then
                         lib=`echo $fallback_file | $SED 's/^-l//;s/^lib//;s/\(.*\)\.a/\1/;s/\(.*\).so/\1/'`
                         # strip any leading -l or lib prefix; any trailing .a
                         # or .so suffixes
                         if ( test X`$EXPR "X $with_Yajl_libpath $with_Yajl_lnklibs " : ".* \(-l$lib\) .*"` == X )
                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Yajl-library and --with-Yajl specify different libraries, --with-Yajl library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-Yajl-library and --with-Yajl specify different libraries, --with-Yajl library setting ignored" >&2;}
                         fi
                      fi

                      if ( test X$fallback_path != X )
                      then
                         # --with_Yajl specified a fallback path, are we using
                         # it?
                         if ( test X`$EXPR "X $with_Yajl_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have any library path; add the fallback
                            trace 3 No path specified by --with-Yajl-library, using --with-Yajl value [-L$fallback_path]
                            with_Yajl_libpath="-L$fallback_path $with_Yajl_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                         else
                            if ( test X`$EXPR "X $with_Yajl_libpath " : ".* \(-L$fallback_path\)/* .*"` == X )
                            then
                               # No -L entry for the fallback path.
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Yajl-library and --with-Yajl specify different paths, --with-Yajl path ignored" >&5
$as_echo "$as_me: WARNING: --with-Yajl-library and --with-Yajl specify different paths, --with-Yajl path ignored" >&2;}
                            fi
                         fi
                      fi
                   fi
                else
                   # --with-Yajl-library used as a switch; employ fallback/default paths
                   if ( test X$fallback_path != X )
                   then
                      trace 3 Using path component of --with-Yajl to set library path for Yajl [-L$fallback_path]
                      with_Yajl_libpath=-L$fallback_path
                      use_Yajl=yes
                   else
                      with_Yajl_libpath=""
                   fi
                   if ( test X$fallback_file != X )
                   then
                         # --with_Yajl specified a fallback name, use that but
                         # sanitise it first
                         lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.a$'`
                         if ( test X$lib != X  )
                         then
                            # Removed an unwanted .a suffix
                            # Now strip any leading -l or lib prefix
                            with_Yajl_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl pointing to a .a" >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                         else
                            lib=`$EXPR "X$fallback_file" : 'X\(.*\)\.so$'`
                            if ( test X$lib != X )
                            then
                               # Removed an unwanted .so suffix
                               # Now strip any leading -l or lib prefix
                               with_Yajl_lnklibs=-l`echo $lib | $SED 's/^-l//;s/^lib//'`
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-Yajl pointing to a .so" >&5
$as_echo "$as_me: WARNING: Specifying --with-Yajl pointing to a .so" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with static libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with static libraries." >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead, we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead, we will use -l$lib" >&2;}
                               { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: " >&5
$as_echo "$as_me: WARNING: " >&2;}
                            else
                               # No suffix to strip
                               # Strip any leading -l or lib prefix
                               with_Yajl_lnklibs=-l`echo $fallback_file | $SED 's/^-l//;s/^lib//'`
                            fi
                         fi
                         trace 3 Using file component of --with-Yajl to set library name for Yajl [$with_Yajl_lnklibs]
                   else
                      trace 3 Using default library name for Yajl ["-lyajl"]
                      with_Yajl_lnklibs="-lyajl"
                   fi
                fi


                # Check if --with-Yajl-include is a path parameter
                if ( test "$with_Yajl_include" != notset && test "$with_Yajl_include" != no && test "$with_Yajl_include" != check && test "$with_Yajl_include" != yes )
                then
                   # we have a parameter rather than a switch to use
                   with_Yajl_inc=""
                   use_Yajl=yes
                   if ( test X$fallback_path != X )
                   then
                      fallback_match=no
                      # If we have a fallback path, we want to check it is
                      # included in the -include setting and warn if not
                   else
                      fallback_match=yes
                      # We have no fallback, so we never need to warn the user
                   fi
                   trace 6 Parsing --with-Yajl-include setting
                   # Strip any provided -I entries for ease of parsing.
                   for path in `echo " $with_Yajl_include " | $SED 's/ -I *//g'`
                   do
                      if ( test -d $path )
                      then
                         TRACE_INDENT="     "
                         trace 6 Appending [-I$path] to set include path for Yajl
                         with_Yajl_inc="$with_Yajl_inc-I$path "
                         if ( test $fallback_match == no && test $fallback_path == $path )
                         then
                            fallback_match=yes
                         fi
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal component of --with-Yajl-include, [$path] ignored" >&5
$as_echo "$as_me: WARNING: Illegal component of --with-Yajl-include, [$path] ignored" >&2;}
                      fi
                   done
                   if ( test X$with_Yajl_inc == X )
                   then
                      # No path from argument, any fallback?
                      if ( test X$fallback_path != X )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use --with-Yajl value [-I$fallback_path]" >&5
$as_echo "$as_me: WARNING: Will use --with-Yajl value [-I$fallback_path]" >&2;}
                         with_Yajl_inc=-I$fallback_path
                      else
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Will use default system include path" >&5
$as_echo "$as_me: WARNING: Will use default system include path" >&2;}
                         with_Yajl_inc=""
                      fi
                   else
                      # We have valid path(es) supplied by the user, do they
                      # encompass the fallback setting?
                      if ( test $fallback_match != yes )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Yajl-include and --with-Yajl specify different paths, --with-Yajl ignored" >&5
$as_echo "$as_me: WARNING: --with-Yajl-include and --with-Yajl specify different paths, --with-Yajl ignored" >&2;}
                      fi
                   fi
                else
                   # --with-Yajl-include is a switch setting rather than a path,
                   # use any available fallback
                   if ( test X$fallback_path != X )
                   then
                      trace 3 No path specified by --with-Yajl-include, using --with-Yajl value [-I$fallback_path]
                      with_Yajl_inc=-I$fallback_path
                      use_Yajl=yes
                   else
                      trace 3 Will use default system include path
                      with_Yajl_inc=""
                   fi
                fi
             fi # End of: if ( test $with_Yajl_check !- pkgconfig
          else
             trace 4 Library Yajl disabled
          fi



          with_Yajl_tstcd='yajl_status ret;
                               yajl_handle h;
                               const unsigned char * txt;
                               unsigned int l;
                               ret=yajl_parse(h, txt, l);
                              '
                                                  with_Yajl_prq=""
          with_Yajl_hdr="yajl/yajl_parse.h"
          with_Yajl_lang="C
                             "





          KnownLibs=$KnownLibs" "Yajl
          if ( test X$use_Yajl = X )
          then
             use_Yajl=check
             UnconfiguredLibs=$UnconfiguredLibs" "Yajl
          else
             ConfiguredLibs=$ConfiguredLibs" "Yajl
          fi






          with_Zlib_check=compile
          if ( test Xcompile = Xvirtual )
          then
             with_Zlib_virtual=yes
          else
             with_Zlib_virtual=no
          fi

          fallback_file=""
          fallback_path=""


# Check whether --with-Zlib-pkgconf was given.
if test "${with_Zlib_pkgconf+set}" = set; then :
  withval=$with_Zlib_pkgconf; case $with_Zlib_pkgconf in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-Zlib-pkgconf disabling Zlib
                                 with_Zlib_pkgconf=no
                                 use_Zlib=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 if ( test X != X )
                                 then
                                    trace 4 Asking for Zlib via --with-Zlib-pkgconf default value [$with_Zlib_pkgconf]
                                    with_Zlib_pkgconf=
                                    with_Zlib_check=pkgconfig
                                    with_Zlib_virtual=no
                                    use_Zlib=yes
                                 else
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable Zlib via pkg-config but configure is unaware of the name of the DOTpc metadata file
See \`config.log' for more details" "$LINENO" 5; }
                                 fi
                                 ;;
                        'c' |'C' | 'ck' | 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 if ( test X != X )
                                 then
                                    trace 4 Will check for Zlib using --with-Zlib-pkgconf default value [$with_Zlib_pkgconf]
                                    with_Zlib_pkgconf=
                                    with_Zlib_check=pkgconfig
                                    with_Zlib_virtual=no
                                    use_Zlib=check
                                 else
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to check for Zlib via pkg-config but configure is unaware of the name of the DOTpc metadata file
See \`config.log' for more details" "$LINENO" 5; }
                                 fi
                                 ;;
                        * )
                                 trace 4 Asking for Zlib --with-Zlib-pkgconf set to [$with_Zlib_pkgconf]
                                 with_Zlib_check=pkgconfig
                                 with_Zlib_virtual=no
                                 use_Zlib=yes
                                 ;;
                       esac

else
  if ( test X != X )
                       then
                          trace 4 No --with-Zlib-pkgconf specified using default [$with_Zlib_pkgconf]
                          with_Zlib_pkgconf=
                          with_Zlib_check=pkgconfig
                          with_Zlib_virtual=no
                       else
                          trace 4 No --with-Zlib-pkgconf specified and no default value
                       fi


fi


# Check whether --with-Zlib was given.
if test "${with_Zlib+set}" = set; then :
  withval=$with_Zlib; case $with_Zlib in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-Zlib disabling Zlib libraries
                                 if ( test X$use_Zlib = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable Zlib by --with-Zlib but --with-Zlib-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Zlib = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling Zlib via --with-Zlib despite --with-Zlib-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling Zlib via --with-Zlib despite --with-Zlib-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Zlib=no
                                 fi
                                 with_Zlib=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-Zlib enabling Zlib libraries
                                 if ( test X$use_Zlib = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable Zlib by --with-Zlib but --with-Zlib-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Zlib = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling Zlib via --with-Zlib despite --with-Zlib-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling Zlib via --with-Zlib despite --with-Zlib-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Zlib=yes
                                 fi
                                 with_Zlib=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-Zlib checking for Zlib libraries
                                 if (test X$use_Zlib = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib asking to check for the libs but --with-Zlib-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-Zlib asking to check for the libs but --with-Zlib-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_Zlib = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Zlib asking to check for the libs but --with-Zlib-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-Zlib asking to check for the libs but --with-Zlib-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_Zlib=check
                                    fi
                                 fi
                                 with_Zlib=check
                                 ;;
                        * )      # --with-<lib> is not being used as a simple
                                 # yes/no/check switch - see if this is
                                 # providing an extra piece of information
                                 if ( test -d $with_Zlib )
                                 then
                                    # Parameter is a directory path
                                    fallback_path=$with_Zlib
                                    fallback_file=""
                                    trace 3 with-Zlib provides a fallback path [$fallback_path]
                                    use_Zlib=yes
                                    with_Zlib=yes
                                 else
                                    fallback_path=`$as_dirname -- $with_Zlib ||
$as_expr X$with_Zlib : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_Zlib : 'X\(//\)[^/]' \| \
	 X$with_Zlib : 'X\(//\)$' \| \
	 X$with_Zlib : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_Zlib |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                    if ( test -d $fallback_path  )
                                    then
                                       # strip leading path
                                       fallback_file=`$EXPR "//$with_Zlib" : '.*/\([^/]*\)'`
                                       # portable version of fallback_file=`basename $with_Zlib`
                                       if ( test X$with_Zlib = X$fallback_path/$fallback_file )
                                       then
                                          # Parameter is a file path
                                          trace 3 with-Zlib provides a fallback path [$fallback_path] and a file [$fallback_file]
                                          use_Zlib=yes
                                          with_Zlib=yes
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-Zlib, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-Zlib, parameter ignored" >&2;}
                                          fallback_path=""
                                          fallback_file=""
                                       fi
                                    else
                                       # --with-Zlib=/path/is/not/a/dir/something
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-Zlib, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-Zlib, parameter ignored" >&2;}
                                       fallback_path=""
                                       fallback_file=""
                                    fi
                                 fi
                                 ;;
                       esac

else
  trace 5 No --with-Zlib specified

fi






# Check whether --with-Zlib-library was given.
if test "${with_Zlib_library+set}" = set; then :
  withval=$with_Zlib_library; case "$with_Zlib_library" in
                          'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-Zlib-library disabling Zlib libraries
                                 if ( test X$use_Zlib = Xyes )
                                 then
                                    if ( test X$with_Zlib_check = Xpkgconfig )
                                    then
                                       # Either --with-<lib>-pkgconf is in use
                                       # or --with-<lib> specifies a .pc file
                                       { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable Zlib via --with-Zlib-library but another configuration option has specifically asked to use pkg-config
See \`config.log' for more details" "$LINENO" 5; }
                                    else
                                       { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable Zlib via --with-Zlib-library but --with-Zlib has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                    fi
                                 else
                                    if ( test X$use_Zlib = Xcheck )
                                    then
                                       # Can only be here if --with-<lib>
