                                    with_Math_lnklibs="${with_Math_lnklibs}-l$lib "
                                 fi
                              fi
                              ;;
                      esac
                   done
                   trace 3 Link line assembled from --with-Math-library is [$with_Math_libpath $with_Math_lnklibs]
                   # Now verify what we have
                   if ( test X`$EXPR "X $with_Math_libpath $with_Math_lnklibs" : '.* \(-l[^ ]*\).*$'` == X )
