                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA-library has specifically enabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA-library has specifically enabled it" >&2;}
                                       else
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA has specifically enabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA has specifically enabled it" >&2;}
                                       fi
                                    fi
                                 else
                                    if ( test X$use_CUDA = Xno )
                                    then
                                       if ( test X$with_CUDA_pkgconf = Xno )
                                       then
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA-pkgconf has specifically disabled it" >&2;}
                                       else
                                          if ( test X$with_CUDA_library = Xno )
                                          then
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA-library has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA-library has specifically disabled it" >&2;}
                                          else
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA has specifically disabled it" >&5
$as_echo "$as_me: WARNING: Asking to check for CUDA via --with-CUDA-include but --with-CUDA has specifically disabled it" >&2;}
                                          fi
                                       fi
                                    else
                                       if ( test X$use_CUDA = Xcheck )
                                       then
                                          # Can never be here because of
                                          # --with-<lib>-pkgconf so we do not
                                          # need to check for any incompatable
                                          # settings
                                          with_CUDA_include=check
                                       else
                                          # No other user setting
                                          use_CUDA=check
                                          # This is the only condition where
                                          # we need to be updating use_<lib>
                                       fi
                                    fi
                                 fi
                                 with_CUDA_include=check
                                 ;;
                          * )
                                 if ( test X$use_CUDA = Xyes )
                                 then
                                    if ( test X$with_CUDA_check = Xpkgconfig )
                                    then
                                       # Either --with-<lib>-pkgconf is in use
                                       # or --with-<lib> specifies a .pc file
                                       { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for CUDA via --with-CUDA-include but another configuration option has specifically asked to use pkg-config
See \`config.log' for more details" "$LINENO" 5; }
                                    fi
                                 else
                                    if ( test X$use_CUDA = Xno )
                                    then
                                       if ( test X$with_CUDA_pkgconf = Xno )
                                       then
                                          { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for CUDA via --with-CUDA-include but --with-CUDA-pkgconf has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                       else
                                          if ( test X$with_CUDA_library = Xno )
                                          then
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for CUDA via --with-CUDA-include but --with-CUDA-library has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          else
                                             { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to use specific header files for CUDA via --with-CUDA-include but --with-CUDA has specifically disabled the package
See \`config.log' for more details" "$LINENO" 5; }
                                          fi
                                       fi
                                    else
                                       if ( test X$use_CUDA = Xcheck )
                                       then
                                          # Can never be here because of
                                          # --with-<lib>-pkgconf
                                          if ( test X$with_CUDA_library = Xcheck )
                                          then
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to use specific header files for CUDA via --with-CUDA-include despite --with-CUDA-library having asked to check for the package" >&5
$as_echo "$as_me: WARNING: Asking to use specific header files for CUDA via --with-CUDA-include despite --with-CUDA-library having asked to check for the package" >&2;}
                                          else
