# Check whether --with-Loudmouth was given.
if test "${with_Loudmouth+set}" = set; then :
  withval=$with_Loudmouth; case $with_Loudmouth in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-Loudmouth disabling Loudmouth libraries
                                 if ( test X$use_Loudmouth = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable Loudmouth by --with-Loudmouth but --with-Loudmouth-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Loudmouth = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling Loudmouth via --with-Loudmouth despite --with-Loudmouth-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling Loudmouth via --with-Loudmouth despite --with-Loudmouth-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Loudmouth=no
                                 fi
                                 with_Loudmouth=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-Loudmouth enabling Loudmouth libraries
                                 if ( test X$use_Loudmouth = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable Loudmouth by --with-Loudmouth but --with-Loudmouth-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Loudmouth = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling Loudmouth via --with-Loudmouth despite --with-Loudmouth-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling Loudmouth via --with-Loudmouth despite --with-Loudmouth-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Loudmouth=yes
                                 fi
                                 with_Loudmouth=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-Loudmouth checking for Loudmouth libraries
                                 if (test X$use_Loudmouth = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Loudmouth asking to check for the libs but --with-Loudmouth-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-Loudmouth asking to check for the libs but --with-Loudmouth-pkgconf has specifically enabled it" >&2;}
                                 else
                                    if ( test X$use_Loudmouth = Xno )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Loudmouth asking to check for the libs but --with-Loudmouth-pkgconf has specifically disabled it" >&5
$as_echo "$as_me: WARNING: --with-Loudmouth asking to check for the libs but --with-Loudmouth-pkgconf has specifically disabled it" >&2;}
                                    else
                                       use_Loudmouth=check
                                    fi
                                 fi
