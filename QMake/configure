                      with_tetgen_inc=-I$fallback_path
                      use_tetgen=yes
                   else
                      trace 3 Will use default system include path
                      with_tetgen_inc=""
                   fi
                fi
             fi # End of: if ( test $with_tetgen_check !- pkgconfig
          else
             trace 4 Library tetgen disabled
          fi



          with_tetgen_tstcd='char * switches;
                               tetgenio * in;
                               tetgenio * out;
