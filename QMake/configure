                   trace 6 gcc version $CC_MAJOR.$CC_MINOR
                   COMPILER_NON_OPT_CFLAGS=" "$COMPILER_NON_OPT_CFLAGS" -fno-strict-aliasing "
                   COMPILER_OPT_CFLAGS=" "$COMPILER_OPT_CFLAGS" -fno-strict-aliasing "
                fi
             else
