# Check whether --with-Qt5Sql was given.
if test "${with_Qt5Sql+set}" = set; then :
  withval=$with_Qt5Sql; case $with_Qt5Sql in
                        'n' | 'N' | 'no' | 'No' | 'NO' )
                                 trace 4 --with-Qt5Sql disabling Qt5Sql libraries
                                 if ( test X$use_Qt5Sql = Xyes )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to disable Qt5Sql by --with-Qt5Sql but --with-Qt5Sql-pkgconf has specifically enabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Qt5Sql = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling Qt5Sql via --with-Qt5Sql despite --with-Qt5Sql-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling Qt5Sql via --with-Qt5Sql despite --with-Qt5Sql-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Qt5Sql=no
                                 fi
                                 with_Qt5Sql=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-Qt5Sql enabling Qt5Sql libraries
                                 if ( test X$use_Qt5Sql = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable Qt5Sql by --with-Qt5Sql but --with-Qt5Sql-pkgconf has specifically disabled it
See \`config.log' for more details" "$LINENO" 5; }
                                 else
                                    if ( test X$use_Qt5Sql = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Enabling Qt5Sql via --with-Qt5Sql despite --with-Qt5Sql-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Enabling Qt5Sql via --with-Qt5Sql despite --with-Qt5Sql-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_Qt5Sql=yes
                                 fi
                                 with_Qt5Sql=yes
                                 ;;
                        'c' | 'C' | 'ck'| 'Ck' | 'CK' | 'check' | 'Check' | 'CHECK' )
                                 trace 4 --with-Qt5Sql checking for Qt5Sql libraries
                                 if (test X$use_Qt5Sql = Xyes )
                                 then
                                    { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Qt5Sql asking to check for the libs but --with-Qt5Sql-pkgconf has specifically enabled it" >&5
$as_echo "$as_me: WARNING: --with-Qt5Sql asking to check for the libs but --with-Qt5Sql-pkgconf has specifically enabled it" >&2;}
                                 else
