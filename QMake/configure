                if ( test "$with_posixThreads_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads specifically disables library but --with-posixThreads-include" >&5
$as_echo "$as_me: WARNING: --with-posixThreads specifically disables library but --with-posixThreads-include" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-posixThreads-include ignored" >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-posixThreads-include ignored" >&2;}
                   use_posixThreads=no
                else
                   # --with-posixThreads-include is yes or points to the include
                   # directory
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-include asking to use posixThreads but --with-posixThreads disabled it
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

          else
             # test --with-posixThreads != no

             if ( test $with_posixThreads = check )
             then
                if ( test $with_posixThreads_pkgconf != no && test $with_posixThreads_pkgconf != notset )
                then
                   # --with-posixThreads-pkgconf must point to the metafile
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for posixThreads via pkg-config but --with-posixThreads is" >&5
$as_echo "$as_me: WARNING: Explicitly asking for posixThreads via pkg-config but --with-posixThreads is" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-posixThreads ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-posixThreads ignored." >&2;}
                   use_posixThreads=yes
                fi

                if ( test "$with_posixThreads_library" != check && test "$with_posixThreads_library" != notset )
                then
                   if ( test "$with_posixThreads_library" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for posixThreads via --with-posixThreads but --with-posixThreads-library" >&5
$as_echo "$as_me: WARNING: Asking to check for posixThreads via --with-posixThreads but --with-posixThreads-library" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-posixThreads ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-posixThreads ignored." >&2;}
                      use_posixThreads=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for posixThreads via --with-posixThreads-library but --with-posixThreads" >&5
$as_echo "$as_me: WARNING: Explicitly asking for posixThreads via --with-posixThreads-library but --with-posixThreads" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-posixThreads ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-posixThreads ignored." >&2;}
                      use_posixThreads=yes
                   fi
                fi

                if ( test "$with_posixThreads_include" != check && test "$with_posixThreads_include" != notset )
                then
                   if ( test "$with_posixThreads_include" = no )
                   then
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Asking to check for posixThreads via --with-posixThreads but --with-posixThreads-include" >&5
$as_echo "$as_me: WARNING: Asking to check for posixThreads via --with-posixThreads but --with-posixThreads-include" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disables it; --with-posixThreads ignored." >&5
$as_echo "$as_me: WARNING: explicitly disables it; --with-posixThreads ignored." >&2;}
                      use_posixThreads=no
                   else
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly asking for posixThreads via --with-posixThreads-include but --with-posixThreads" >&5
$as_echo "$as_me: WARNING: Explicitly asking for posixThreads via --with-posixThreads-include but --with-posixThreads" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-posixThreads ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-posixThreads ignored." >&2;}
                      use_posixThreads=yes
                   fi
                fi

             else
                # --with-posixThreads != no && != check
                if ( test $with_posixThreads != notset )
                then
                   # --with-posixThreads = path or yes

                   # We need not check --with-posixThreads-pkgconf as there is no
                   # possible conflict (cannot be check and we take no to
                   # be a request to disable pkg-config, rather than the
                   # library as a whole)

                   if ( test "$with_posixThreads_library" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-library disabling posixThreads but --with-posixThreads enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_posixThreads_library" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling posixThreads via --with-posixThreads but --with-posixThreads-library" >&5
$as_echo "$as_me: WARNING: Explicitly enabling posixThreads via --with-posixThreads but --with-posixThreads-library" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-posixThreads-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-posixThreads-include ignored." >&2;}
                         use_posixThreads=no
                      fi
                   fi

                   if ( test "$with_posixThreads_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-include disabling posixThreads but --with-posixThreads enabled it
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_posixThreads_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Explicitly enabling posixThreads via --with-posixThreads but --with-posixThreads-include" >&5
$as_echo "$as_me: WARNING: Explicitly enabling posixThreads via --with-posixThreads but --with-posixThreads-include" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING:  asking to check for it; --with-posixThreads-include ignored." >&5
$as_echo "$as_me: WARNING:  asking to check for it; --with-posixThreads-include ignored." >&2;}
                         use_posixThreads=no
                      fi
                   fi
                fi
             fi
          fi

          # Now test --with-posixThreads-pkgconf is not in conflict with the other two
          # command-line switches.
          if ( test $with_posixThreads_pkgconf != notset && test $with_posixThreads_pkgconf != no )
          then
             # --with-posixThreads-pkgconf = path (cannot be yes or check)
             if ( test "$with_posixThreads_library" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads-library asking to check for posixThreads but --with-posixThreads-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-posixThreads-library asking to check for posixThreads but --with-posixThreads-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-posixThreads-library ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-posixThreads-library ignored." >&2;}
                use_posixThreads=yes
             else
                if ( test "$with_posixThreads_library" != no && test "$with_posixThreads_library" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-library enabling compile check for posixThreads but --with-posixThreads-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi

             if ( test "$with_posixThreads_include" = check )
             then
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads-include asking to check for posixThreads but --with-posixThreads-pkgconf asking" >&5
$as_echo "$as_me: WARNING: --with-posixThreads-include asking to check for posixThreads but --with-posixThreads-pkgconf asking" >&2;}
                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: to use pkg-config, --with-posixThreads-include ignored." >&5
$as_echo "$as_me: WARNING: to use pkg-config, --with-posixThreads-include ignored." >&2;}
                use_posixThreads=yes
             else
                if ( test "$with_posixThreads_include" != no && test "$with_posixThreads_include" != notset )
                then
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-include enabling compile check for posixThreads but --with-posixThreads-pkgconf asking to use pkg-config
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          fi


          # Check consistency between library and include switches
          if ( test "$with_posixThreads_library" = no )
          then
             if ( test "$with_posixThreads_include" != no && test "$with_posixThreads_include" != notset )
             then
                if ( test "$with_posixThreads_include" = check )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads-include asking to check for posixThreads but --with-posixThreads-library has" >&5
$as_echo "$as_me: WARNING: --with-posixThreads-include asking to check for posixThreads but --with-posixThreads-library has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-posixThreads-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-posixThreads-include ignored." >&2;}
                   use_posixThreads=no
                else
                   # --with-posixThreads-include = yes or path
                   { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-include enabling posixThreads but --with-posixThreads-library disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                fi
             fi
          else
             # --with-posixThreads-library != no
             if ( test "$with_posixThreads_library" = check )
             then
                if ( test "$with_posixThreads_include" = no )
                then
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads-library asking to check for posixThreads but --with-posixThreads-include has" >&5
$as_echo "$as_me: WARNING: --with-posixThreads-library asking to check for posixThreads but --with-posixThreads-include has" >&2;}
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly disabled it; --with-posixThreads-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly disabled it; --with-posixThreads-library ignored." >&2;}
                   use_posixThreads=no
                else
                   if ( test "$with_posixThreads_include" != check && test "$with_posixThreads_include" != notset )
                   then
                      # --with-posixThreads-include = yes or path
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads-library asking to check for posixThreads but --with-posixThreads-include has" >&5
$as_echo "$as_me: WARNING: --with-posixThreads-library asking to check for posixThreads but --with-posixThreads-include has" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-posixThreads-library ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-posixThreads-library ignored." >&2;}
                      use_posixThreads=yes
                   fi
                fi
             else
                if ( test "$with_posixThreads_library" != notset )
                then
                   # --with-posixThreads-library a path or yes
                   if ( test "$with_posixThreads_include" = no )
                   then
                      { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error "--with-posixThreads-library enabling posixThreads but --with-posixThreads-include disabled it.
See \`config.log' for more details." "$LINENO" 5; }
                   else
                      if ( test "$with_posixThreads_include" = check )
                      then
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-posixThreads-include asking to check for posixThreads but --with-posixThreads-library has" >&5
$as_echo "$as_me: WARNING: --with-posixThreads-include asking to check for posixThreads but --with-posixThreads-library has" >&2;}
                         { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: explicitly enabled it; --with-posixThreads-include ignored." >&5
$as_echo "$as_me: WARNING: explicitly enabled it; --with-posixThreads-include ignored." >&2;}
                         use_posixThreads=yes
                      fi
                   fi
                fi
             fi
          fi


          # Now process --with-posixThreads to see if it is a fallback path
          #
          # Initialise fallback components
          fallback_path=""
          fallback_file=""
          # Check for viable switch setting
          if ( test $with_posixThreads != notset && test $with_posixThreads != no && test $with_posixThreads != check && test $with_posixThreads != yes )
          then
             # We have a path to use
             if ( test -d $with_posixThreads )
             then
                # Parameter is a directory path
                fallback_path=$with_posixThreads
                trace 3 with-posixThreads provides a fallback path [$fallback_path]
             else
                fallback_path=`$as_dirname -- $with_posixThreads ||
$as_expr X$with_posixThreads : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$with_posixThreads : 'X\(//\)[^/]' \| \
	 X$with_posixThreads : 'X\(//\)$' \| \
	 X$with_posixThreads : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$with_posixThreads |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                if ( test -d $fallback_path  )
                then
                   # strip leading path
                   fallback_file=`$EXPR "//$with_posixThreads" : '.*/\([^/]*\)'`
                   # portable version of fallback_file=`basename $with_posixThreads`
                   if ( test X$with_posixThreads = X$fallback_path/$fallback_file )
                   then
                      # Parameter is a file path
                      trace 3 with-posixThreads provides a fallback path [$fallback_path] and a file [$fallback_file]
                   else
                      # Somehow dirname($x)/basename($x) != $x
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-posixThreads, parameter ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-posixThreads, parameter ignored" >&2;}
                      fallback_path=""
                      fallback_file=""
                   fi
                else
                   # --with-posixThreads=/path/is/not/a/dir/something
                   { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal command-line option for --with-posixThreads, parameter ignored" >&5
$as_echo "$as_me: WARNING: Illegal command-line option for --with-posixThreads, parameter ignored" >&2;}
                   fallback_path=""
                   fallback_file=""
                fi
             fi
          fi


          if ( test X$use_posixThreads != Xno )
          then
             if ( test $with_posixThreads_pkgconf = no ||  test $with_posixThreads_pkgconf = notset )
             then
                # Not using pkg-config, set any paths for compile checks

                # Check if --with-posixThreads-library is a specification rather than a
                # simple switch
                if ( test "$with_posixThreads_library" != notset && test "$with_posixThreads_library" != no && test "$with_posixThreads_library" != check && test "$with_posixThreads_library" != yes )
                then
                   # We have a path specification to parse
                   #
                   # If the specification is a straight forward link line (of
                   # the form "-L/a/b/c -ld .. -L/w/x/y -lz" we want to split
                   # off the last -l entries into with_posixThreads_lnklibs and include
                   # the initial part of the link line in with_posixThreads_libpath.
                   # If we just have directory and library paths, we need to
                   # add the relevant linker switch, and assign the parameter
                   # to the appropriate variable.
                   #
                   with_posixThreads_lnklibs=""
                   with_posixThreads_libpath=""
                   use_posixThreads=yes
                   trace 6 Parsing --with-posixThreads-library setting
                   with_posixThreads_library=`echo " $with_posixThreads_library " | $SED 's/ -L */ -L/g;s/ -l */ -l/g'`
                   # Collapse down spaces following -L and -l entries. This is
                   # mainly for ease of parsing but does make any user input
                   # compatible with the form of the default values used
                   # throughout this configure script. Downsides are that any
                   # other -X <arg> switches provided by the user will not
                   # parse correctly and a general issue awaiting any systems
                   # that must have the space between the switch and its
                   # argument. The latter issue is the bigger problem as such
                   # a limitation will have big consequences for this script
                   # if we ever hit such a constrained toolset.
                   for part in $with_posixThreads_library
                   do
                      case $part in
                      -L* )   # Library path specifier. Any previous library
                              # names ought to be recorded as part of libpath
                              # to maintain any order significance of the
                              # supplied link command.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library path
                              with_posixThreads_libpath="$with_posixThreads_libpath$with_posixThreads_lnklibs$part "
                              with_posixThreads_lnklibs=""
                              ;;
                      -l* )   # Library specifier. Append to the linklib
                              # variable.
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to library list
                              with_posixThreads_lnklibs="$with_posixThreads_lnklibs$part "
                              ;;
                      -* )    # Presume linker switch. Append to libpath after
                              # adding any existing library specs (so as to
                              # maintain any order specific requirements).
                              TRACE_INDENT="     "
                              trace 6 Appending [$part] to link line
                              with_posixThreads_libpath="$with_posixThreads_libpath$with_posixThreads_lnklibs$part "
                              with_posixThreads_lnklibs=""
                              ;;
                      * )     # Non-linker format, see if we can work out
                              # what it is
                              if ( test -d $part )
                              then
                                 # We have a directory, add to libpath with
                                 # a -L prefix having moved across any
                                 # libraries from lnklibs to preserve ordering
                                 # constraints
                                 TRACE_INDENT="     "
                                 trace 6 Adding [-L$part] to link line
                                 with_posixThreads_libpath="$with_posixThreads_libpath${with_posixThreads_lnklibs}-L$part "
                                 with_posixThreads_lnklibs=""
                              else
                                 # Not a directory, is it directory/file?
                                 path=`$as_dirname -- $part ||
$as_expr X$part : 'X\(.*[^/]\)//*[^/][^/]*/*$' \| \
	 X$part : 'X\(//\)[^/]' \| \
	 X$part : 'X\(//\)$' \| \
	 X$part : 'X\(/\)' \| . 2>/dev/null ||
$as_echo X$part |
    sed '/^X\(.*[^/]\)\/\/*[^/][^/]*\/*$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)[^/].*/{
	    s//\1/
	    q
	  }
	  /^X\(\/\/\)$/{
	    s//\1/
	    q
	  }
	  /^X\(\/\).*/{
	    s//\1/
	    q
	  }
	  s/.*/./; q'`
                                 if ( test X$path != X && test $path != . )
                                 then
                                    # We have "something/file" does the
                                    # "something" point to a directory?
                                    if ( test -d $path  )
                                    then
                                       # We have "directory/presumed-library-name"
                                       # Find candidate library name
                                       file=`$EXPR "//$part" : '.*/\([^/]*\)'`
                                       if ( test $part = $path/$file )
                                       then
                                          # Parameter is a file path
                                          # Does it have a '.a' extension
                                          lib=`$EXPR "X$file" : 'X\(.*\)\.a$'`
                                          if ( test X$lib != X  )
                                          then
                                             lib=`echo $lib | $SED 's/^lib//'`
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-posixThreads_library pointing to a .a" >&5
$as_echo "$as_me: WARNING: Specifying --with-posixThreads_library pointing to a .a" >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: will cause problems building with shared libraries." >&5
$as_echo "$as_me: WARNING: will cause problems building with shared libraries." >&2;}
                                             { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Instead we will use -l$lib" >&5
$as_echo "$as_me: WARNING: Instead we will use -l$lib" >&2;}
                                          else
                                             # Does it have a '.so' extension
                                             lib=`$EXPR "X$file" : 'X\(.*\)\.so$'`
                                             if ( test X$lib != X )
                                             then
                                                lib=`echo $lib | $SED 's/^lib//'`
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Specifying --with-posixThreads_library pointing to" >&5
$as_echo "$as_me: WARNING: Specifying --with-posixThreads_library pointing to" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: a .so will cause problems building with " >&5
$as_echo "$as_me: WARNING: a .so will cause problems building with " >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: static libraries. Instead, we will use" >&5
$as_echo "$as_me: WARNING: static libraries. Instead, we will use" >&2;}
                                                { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: -l$lib" >&5
$as_echo "$as_me: WARNING: -l$lib" >&2;}
                                             else
                                                # Parameter is path and name fragment
                                                lib=`echo $file | $SED 's/^lib//'`
                                             fi
                                          fi
                                          # As the user specified the library
                                          # as a path rather than with a -L
                                          # and a -l entry, it would be good
                                          # to continue in the same vein.
                                          # However, as -l/full/path is not
                                          # supported, we have to revert to
                                          # the seperate specifiers.
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-L$path] to library path
                                          with_posixThreads_libpath="$with_posixThreads_libpath$with_posixThreads_lnklibs-L$path "
                                          with_posixThreads_lnklibs=""
                                          TRACE_INDENT="     "
                                          trace 6 Adding [-l$lib] to library list
                                          with_posixThreads_lnklibs="-l$lib "
                                       else
                                          # Somehow dirname($x)/basename($x) != $x
                                          { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Problem processing --with-posixThreads-library, [$part] ignored" >&5
$as_echo "$as_me: WARNING: Problem processing --with-posixThreads-library, [$part] ignored" >&2;}
                                       fi
                                    else
                                       # Parameter is "not-a-dir-path/something"
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Illegal value for --with-posixThreads-library, [$part] ignored" >&5
