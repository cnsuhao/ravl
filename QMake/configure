                                 else
                                    if ( test X$use_FFMPEGSWS = Xcheck )
                                    then
                                       { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Disabling FFMPEGSWS via --with-FFMPEGSWS despite --with-FFMPEGSWS-pkconfig having asked to check for it" >&5
$as_echo "$as_me: WARNING: Disabling FFMPEGSWS via --with-FFMPEGSWS despite --with-FFMPEGSWS-pkconfig having asked to check for it" >&2;}
                                    fi
                                    use_FFMPEGSWS=no
                                 fi
                                 with_FFMPEGSWS=no
                                 ;;
                        'y' |'Y' | 'yes' | 'Yes' | 'YES' )
                                 trace 4 --with-FFMPEGSWS enabling FFMPEGSWS libraries
                                 if ( test X$use_FFMPEGSWS = Xno )
                                 then
                                    { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "Asking to enable FFMPEGSWS by --with-FFMPEGSWS but --with-FFMPEGSWS-pkgconf has specifically disabled it
