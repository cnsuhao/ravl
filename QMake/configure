                         if ( test X`$EXPR "X $with_Math_libpath " : '.* \(-L[^ ]*\).*$'` == X )
                         then
                            # We do not have a library path; add the fallback
                            with_Math_libpath="-L$fallback_path $with_Math_libpath"
                            # It is an assumption to put it first, but we do
                            # not know where the relevant library is in the
                            # link specification, so we have to put it there.
                            trace 3 No path component of --with-Math-library, using --with-Math to set library path for Math [-L$fallback_path]
                         else
