                         then
                            # No -l<fallback>
                            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: --with-Socket-library and --with-Socket specify different libraries, --with-Socket library setting ignored" >&5
$as_echo "$as_me: WARNING: --with-Socket-library and --with-Socket specify different libraries, --with-Socket library setting ignored" >&2;}
