$as_echo "$as_me: WARNING: Explicitly asking for Math via --with-Math-include but --with-Math" >&2;}
                      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: asking to check for it; --with-Math ignored." >&5
$as_echo "$as_me: WARNING: asking to check for it; --with-Math ignored." >&2;}
